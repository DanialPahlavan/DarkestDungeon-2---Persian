msgid "*desperate wheezing*"
msgstr "*відчайдушно хрипить*"

msgctxt "bark_bark_disease_coughing_fit@2"
msgid "*dry, hacking cough*"
msgstr "*сухий уривчастий кашель*"

msgctxt "bark_bark_disease_coughing_fit@3"
msgid "Khkk... let me catch my breath..."
msgstr "Кх... треба перепочити..."

