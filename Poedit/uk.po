msgctxt "affinity_actor_tooltip_envious"
msgid "<color=#{path_verbose}><i>{0} envies {1} and not in a healthy way.</i></color>"
msgstr "<color=#{path_verbose}><i>{0} та {1} люто заздрять одне одному.</i></color>"

msgctxt "affinity_actor_tooltip_hateful"
msgid "<color=#{path_verbose}><i>{0} despises {1} and wishes harm to befall them.</i></color>"
msgstr "<color=#{path_verbose}><i>{0} та {1} зневажають одне одного й бажають лише поганого.</i></color>"

msgctxt "affinity_actor_tooltip_hopeful"
msgid ""
"<color=#{path_verbose}><i>{0} trusts in {1} and believes that together they can conquer any foe.</i></color>"
msgstr "<color=#{path_verbose}><i>{0} та {1} довіряють одне одному й вважають, що разом здолають будь-кого.</i></color>"

msgctxt "affinity_actor_tooltip_leaning_level_friendly"
msgid "<color=#{notable}>Friendly:</color> Possible to form a positive relationship."
msgstr "<color=#{notable}>Дружелюбність:</color> є ймовірність покращити стосунки."

msgctxt "affinity_actor_tooltip_leaning_level_maximum"
msgid "<color=#{notable}>Maximum:</color> Extremely likely to form a positive relationship."
msgstr "<color=#{notable}>Максимум:</color> є дуже велика ймовірність покращити стосунки."

msgctxt "affinity_actor_tooltip_leaning_level_minimum"
msgid "<color=#{harmful}>Minimum:</color> Extremely likely to form a negative relationship."
msgstr "<color=#{harmful}>Мінімум:</color> є дуже велика ймовірність погіршити стосунки."

msgctxt "affinity_actor_tooltip_leaning_level_neutral"
msgid "<color=#{neutral}>Neutral:</color> Unlikely to form a relationship."
msgstr "<color=#{neutral}>Байдужість:</color> не схоже, що є ймовірність налагодити стосунки."

msgctxt "affinity_actor_tooltip_leaning_level_unfriendly"
msgid "<color=#{harmful}>Unfriendly:</color> Possible to form a negative relationship."
msgstr "<color=#{harmful}>Неприязнь:</color> є ймовірність погіршити стосунки."

msgctxt "affinity_actor_tooltip_leaning_level_very_friendly"
msgid "<color=#{notable}>Very Friendly:</color> Likely to form a positive relationship."
msgstr "<color=#{notable}>Симпатія:</color> є велика ймовірність покращити стосунки."

msgctxt "affinity_actor_tooltip_leaning_level_very_unfriendly"
msgid "<color=#{harmful}>Very Unfriendly:</color> Likely to form a negative relationship."
msgstr "<color=#{harmful}>Ворожість:</color> є велика ймовірність погіршити стосунки."

msgctxt "affinity_actor_tooltip_resentful"
msgid "<color=#{path_verbose}><i>{0} resents {1} and feels they lack worth.</i></color>"
msgstr "<color=#{path_verbose}><i>{0} та {1} ображаються одне на одного й вважають, що заслуговують на краще ставлення.</i></color>"

msgctxt "affinity_actor_tooltip_respectful"
msgid ""
"<color=#{path_verbose}><i>{0} respects {1} and is deeply confident that they will carry their weight.</i></"
"color>"
msgstr "<color=#{path_verbose}><i>{0} та {1} поважають одне одного й щиро вважають, що разом можуть усе.</i></color>"

msgctxt "affinity_actor_tooltip_suspicious"
msgid ""
"<color=#{path_verbose}><i>{0} does not trust {1} and suspects they are secretly working against the party.</"
"i></color>"
msgstr "<color=#{path_verbose}><i>{0} та {1} не довіряють одне одному й підозрюють в зраді.</i></color>"

msgctxt "affinity_actor_tooltip_tumultuous"
msgid "Tumultuous {0} {1}"
msgstr "Напруження {0} {1}"

msgctxt "affinity_change_bark_add_buff_observing_jealous@0"
msgid "Clearly I've no part in your plan."
msgstr "Мене явно не було в твоїх планах."

msgctxt "affinity_change_bark_add_buff_observing_jealous@1"
msgid "Forgotten in my hour of need."
msgstr "Мене забули в скрутну годину."

msgctxt "affinity_change_bark_add_buff_observing_jealous+flagellant"
msgid "You lack clarity and vision!"
msgstr "Тобі бракує розважливості й далекоглядності!"

msgctxt "affinity_change_bark_add_buff_observing_jealous+grave_robber"
msgid "Hmph! Suit yourself."
msgstr "Уф... Роби як вважаєш за краще."

msgctxt "affinity_change_bark_add_buff_observing_jealous+hellion"
msgid "Soft-skull!"
msgstr "Слабодух!"

msgctxt "affinity_change_bark_add_buff_observing_jealous+highwayman"
msgid "I see how it is."
msgstr "Он як..."

msgctxt "affinity_change_bark_add_buff_observing_jealous+jester"
msgid "Am I a joke to you?"
msgstr "А я що, нікчема?"

msgctxt "affinity_change_bark_add_buff_observing_jealous+leper"
msgid "Swift to aid the others..."
msgstr "Іншим допомагаєш, аж гуде..."

msgctxt "affinity_change_bark_add_buff_observing_jealous+man_at_arms"
msgid "Tactically unwise!"
msgstr "Не тактика, а казна-що!"

msgctxt "affinity_change_bark_add_buff_observing_jealous+occultist"
msgid "You may live to regret that."
msgstr "Колись ти про це пожалкуєш."

msgctxt "affinity_change_bark_add_buff_observing_jealous+plague_doctor"
msgid "By my calculations, you misjudged that."
msgstr "Як на мене, це якась хиба."

msgctxt "affinity_change_bark_add_buff_observing_jealous+runaway"
msgid "Nobody listens to me anyway."
msgstr "Однаково мене ніхто не слухає."

msgctxt "affinity_change_bark_add_buff_observing_jealous+vestal"
msgid "Think yourself so wise, hm?"
msgstr "Вважаєш себе краще за інших?"

msgctxt "affinity_change_bark_add_buff_participating@0"
msgid "Let me lend a hand."
msgstr "Давай допоможу."

msgctxt "affinity_change_bark_add_buff_participating@1"
msgid "This might be of some help!"
msgstr "Це може допомогти."

msgctxt "affinity_change_bark_add_buff_participating+flagellant"
msgid "The divine flows through me!"
msgstr "Божественне тече крізь мене!"

msgctxt "affinity_change_bark_add_buff_participating+grave_robber"
msgid "One of my finest powders. I insist."
msgstr "Один із моїх найкращих порошків. Бери."

msgctxt "affinity_change_bark_add_buff_participating+hellion"
msgid "Spirits at your back!"
msgstr "Позаду тебе духи!"

msgctxt "affinity_change_bark_add_buff_participating+highwayman"
msgid "If you get the shot, take it."
msgstr "Це треба приймати, коли поранять."

msgctxt "affinity_change_bark_add_buff_participating+jester"
msgid "Oh, I'll cheer you on if I <i>must.</i>"
msgstr "Я тебе підтримаю, якщо <i>буде потрібно.</i>"

msgctxt "affinity_change_bark_add_buff_participating+leper"
msgid "Find strength in the fray, friend."
msgstr "Сила приходить під час бою!"

msgctxt "affinity_change_bark_add_buff_participating+man_at_arms"
msgid "Steel yerself!"
msgstr "Тримайся!"

msgctxt "affinity_change_bark_add_buff_participating+occultist"
msgid "The stars are, at times, benevolent."
msgstr "Інколи зірки до нас прихильні."

msgctxt "affinity_change_bark_add_buff_participating+plague_doctor"
msgid "Sniff this - to balance the humours!"
msgstr "Нюхни — це покращить настрій!"

msgctxt "affinity_change_bark_add_buff_participating+runaway"
msgid "Don't say I never gave you nothin'."
msgstr "І не кажи, що я нічим із тобою не ділюся."

msgctxt "affinity_change_bark_add_buff_participating+vestal"
msgid "A blessing, if you're in need."
msgstr "Двічі дає той, хто дає вчасно."

msgctxt "affinity_change_bark_add_guard_jealous_wounded@0"
msgid "I see that I must get by without your help."
msgstr "Обійдусь без твоєї допомоги."

msgctxt "affinity_change_bark_add_guard_jealous_wounded@1"
msgid "You see me suffer and do nothing...!"
msgstr "Бачиш, що мені зле, і нічого не робиш..."

msgctxt "affinity_change_bark_add_guard_jealous_wounded+flagellant"
msgid "Only I decide when I bleed!"
msgstr "Тільки я вирішую, коли мені спливати кров’ю!"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+grave_robber"
msgid "I shall continue on bleeding, I suppose."
msgstr "Мабуть, я буду й далі спливати кров’ю."

msgctxt "affinity_change_bark_add_guard_jealous_wounded+hellion"
msgid "I cannot fight if you let me die!"
msgstr "Я не зможу битися, якщо ти даси мені померти!"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+highwayman"
msgid "No aid over here?"
msgstr "І що, ніяк не допоможеш?"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+jester"
msgid "Did I tick you off somehow?"
msgstr "Я тебе чимось дратую?"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+leper"
msgid "*quiet grumble*"
msgstr "*тихо буркотить*"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+man_at_arms"
msgid "You deploy our strength unwisely!"
msgstr "Ти використовуєш нашу силу нерозумно!"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+occultist"
msgid "A fell omen, that."
msgstr "Ось воно, погане знамення."

msgctxt "affinity_change_bark_add_guard_jealous_wounded+plague_doctor"
msgid "Inefficient!"
msgstr "Ніякої користі!"

msgctxt "affinity_change_bark_add_guard_jealous_wounded+runaway"
msgid "Good thing I expected no better."
msgstr "На щось краще я й не сподівалась."

msgctxt "affinity_change_bark_add_guard_jealous_wounded+vestal"
msgid "I surely have greater need!"
msgstr "Мені точно це потрібно більше!"

msgctxt "affinity_change_bark_add_guard_mortally_jealous@0"
msgid "I sit on the brink - and for what?"
msgstr "Я стою на краю прірви... заради чого?"

msgctxt "affinity_change_bark_add_guard_mortally_jealous@1"
msgid "If this be the end, you'll regret it."
msgstr "Якщо це кінець, ти пошкодуєш."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+flagellant"
msgid "You would let my suffering go to waste?!"
msgstr "Невже мої страждання пропадуть даремно?!"

msgctxt "affinity_change_bark_add_guard_mortally_jealous+grave_robber"
msgid "I deserved better."
msgstr "Я заслуговувала на краще."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+hellion"
msgid "Buffoon!"
msgstr "Посміховисько!"

msgctxt "affinity_change_bark_add_guard_mortally_jealous+highwayman"
msgid "Mistake it was, counting on you."
msgstr "Розраховувати на тебе було помилкою."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+jester"
msgid "I'll carry on alone, thanks."
msgstr "Я і сам впораюся, дякую."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+leper"
msgid "You sentence me to death."
msgstr "Ти виносиш мені смертний вирок."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+man_at_arms"
msgid "If we lose this day, it's at your feet!"
msgstr "Якщо ми програємо, то з твоєї вини."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+occultist"
msgid "You would make a sacrifice of me? I see."
msgstr "Збираєшся пожертвувати мною? Зрозуміло..."

msgctxt "affinity_change_bark_add_guard_mortally_jealous+plague_doctor"
msgid "If I bleed out, who will staunch <i>your</i> wounds?"
msgstr "Якщо я спливу кров’ю, хто вилікує <i>твої</i> рани?"

msgctxt "affinity_change_bark_add_guard_mortally_jealous+runaway"
msgid "What am I worth to you?!"
msgstr "Хто я для тебе?"

msgctxt "affinity_change_bark_add_guard_mortally_jealous+vestal"
msgid "If I expire, you shall rue the day."
msgstr "Якщо я помру, ти пошкодуєш!"

msgctxt "affinity_change_bark_add_guard_participating@0"
msgid "Behind me!"
msgstr "Ховайся за мене!"

msgctxt "affinity_change_bark_add_guard_participating@1"
msgid "I'll draw them over!"
msgstr "Я відверну їх увагу!"

msgctxt "affinity_change_bark_add_guard_participating@2"
msgid "They won't get past me!"
msgstr "Ніхто не пройде повз мене!"

msgctxt "affinity_change_bark_add_guard_participating_dd@0"
msgid "Hold fast!"
msgstr "Не здавайся!"

msgctxt "affinity_change_bark_add_guard_participating_dd@1"
msgid "It isn't your time, not yet."
msgstr "Твій час ще не настав."

msgctxt "affinity_change_bark_add_guard_participating_dd+flagellant"
msgid "Have at me, pale rider! I've bested you once!"
msgstr "Подивись на мене, вершнице на блідому коні! Одного разу я тебе вже переміг!"

msgctxt "affinity_change_bark_add_guard_participating_dd+grave_robber"
msgid "Look at you - inspiring heroics!"
msgstr "Ти справжнє втілення героїзму!"

msgctxt "affinity_change_bark_add_guard_participating_dd+hellion"
msgid "Hold fast - I am your armour!"
msgstr "Тримайся, я закрию тебе собою!"

msgctxt "affinity_change_bark_add_guard_participating_dd+highwayman"
msgid "It doesn't end here. "
msgstr "Це не кінець. "

msgctxt "affinity_change_bark_add_guard_participating_dd+jester"
msgid "Allow me to draw their ire."
msgstr "Дай-но я спрямую на себе їхню лють."

msgctxt "affinity_change_bark_add_guard_participating_dd+leper"
msgid "Death is not to be feared, but let us not hurry it."
msgstr "Смерті не треба боятись, але й наближати її не варто."

msgctxt "affinity_change_bark_add_guard_participating_dd+man_at_arms"
msgid "Retreat if ye can!"
msgstr "Відступай, якщо можеш!"

msgctxt "affinity_change_bark_add_guard_participating_dd+occultist"
msgid "Fate has other things in store for you."
msgstr "Доля має інші плани на тебе."

msgctxt "affinity_change_bark_add_guard_participating_dd+plague_doctor"
msgid "Bodies can be mended!"
msgstr "Тіло можна підлатати!"

msgctxt "affinity_change_bark_add_guard_participating_dd+runaway"
msgid "Get up, damn you! You're not done!"
msgstr "Та піднімайся ж ти! Це ще не кінець!"

msgctxt "affinity_change_bark_add_guard_participating_dd+vestal"
msgid "Hold on! A better end awaits us!"
msgstr "Тримайся! На нас чекає краща доля!"

msgctxt "affinity_change_bark_add_guard_participating_wounded@0"
msgid "Fear them not while I yet stand!"
msgstr "Не бійтеся нікого, доки я на ногах!"

msgctxt "affinity_change_bark_add_guard_participating_wounded@1"
msgid "Tend to your wounds. I'll hold them off!"
msgstr "Займися своїми ранами. Я прикрию!"

msgctxt "affinity_change_bark_add_guard_participating_wounded+flagellant"
msgid "Death shall be no master over you!"
msgstr "Смерть тебе не отримає!"

msgctxt "affinity_change_bark_add_guard_participating_wounded+grave_robber"
msgid "Don't squander my altruism, hm?"
msgstr "Не знецінюй мій альтруїзм."

msgctxt "affinity_change_bark_add_guard_participating_wounded+hellion"
msgid "My rage is yours!"
msgstr "Моя несамовитість — твоя сила!"

msgctxt "affinity_change_bark_add_guard_participating_wounded+highwayman"
msgid "I'll hold 'em while you right yourself."
msgstr "Я їх затримаю, доки ти не прийдеш до тями."

msgctxt "affinity_change_bark_add_guard_participating_wounded+jester"
msgid "Relax. I'm sturdier than I look."
msgstr "Не переймайся. Мене не так легко здолати."

msgctxt "affinity_change_bark_add_guard_participating_wounded+leper"
msgid "At my side, no harm will reach you."
msgstr "Тобі ніщо не загрожує, доки ти зі мною."

msgctxt "affinity_change_bark_add_guard_participating_wounded+man_at_arms"
msgid "I trained with this shield for a reason!"
msgstr "Недарма я вчився бою зі щитом."

msgctxt "affinity_change_bark_add_guard_participating_wounded+occultist"
msgid "Let the unseen ward us..."
msgstr "Нехай незримі тіні охороняють нас..."

msgctxt "affinity_change_bark_add_guard_participating_wounded+plague_doctor"
msgid "Wounded, yes, but breathing!"
msgstr "Рани є, але життю не загрожують."

msgctxt "affinity_change_bark_add_guard_participating_wounded+runaway"
msgid "I won't let 'em catch you!"
msgstr "Я не дозволю їм тебе спіймати!"

msgctxt "affinity_change_bark_add_guard_participating_wounded+vestal"
msgid "Take shelter behind me."
msgstr "Ховайся за мене."

msgctxt "affinity_change_bark_add_guard_participating+flagellant"
msgid "Face me instead, blackguards!"
msgstr "Краще бийтеся зі мною, негідники!"

msgctxt "affinity_change_bark_add_guard_participating+grave_robber"
msgid "I'll cut down any who try."
msgstr "Уб’ю кожного, хто стане на шляху."

msgctxt "affinity_change_bark_add_guard_participating+hellion"
msgid "Try your luck with me, fiends!"
msgstr "Ну, хто перший, підлотники?"

msgctxt "affinity_change_bark_add_guard_participating+highwayman"
msgid "I've got you."
msgstr "Я тебе прикрию."

msgctxt "affinity_change_bark_add_guard_participating+jester"
msgid "Oho, not without a fight."
msgstr "Я не здамся без бою!"

msgctxt "affinity_change_bark_add_guard_participating+leper"
msgid "A momentary respite."
msgstr "Хвилинний перепочинок."

msgctxt "affinity_change_bark_add_guard_participating+man_at_arms"
msgid "Behind my shield! Now!"
msgstr "За щит! Швидко!"

msgctxt "affinity_change_bark_add_guard_participating+occultist"
msgid "If you'd permit my assistance..."
msgstr "Якщо дозволиш допомогти..."

msgctxt "affinity_change_bark_add_guard_participating+plague_doctor"
msgid "Stand back! I'm testing a method...!"
msgstr "Відійди вбік. Я дещо спробую..."

msgctxt "affinity_change_bark_add_guard_participating+runaway"
msgid "Quick, get down!"
msgstr "Швидше ховайся!"

msgctxt "affinity_change_bark_add_guard_participating+vestal"
msgid "You are under my aegis."
msgstr "Ти під моїм захистом."

msgctxt "affinity_change_bark_add_token_observing_jealous@0"
msgid "All this talk of teamwork..."
msgstr "Командна робота, що й казати..."

msgctxt "affinity_change_bark_add_token_observing_jealous@1"
msgid "Am I of no use...?"
msgstr "Від мене жодної користі?"

msgctxt "affinity_change_bark_add_token_observing_jealous@2"
msgid "Forgot about me?"
msgstr "А як же я?"

msgctxt "affinity_change_bark_add_token_observing_jealous@3"
msgid "Overlooked once again."
msgstr "Знов ніхто мене не помічає."

msgctxt "affinity_change_bark_add_token_observing_jealous+flagellant"
msgid "Your heart cowers from me! I see the truth!"
msgstr "Твоє боїшся мене! Я це бачу!"

msgctxt "affinity_change_bark_add_token_observing_jealous+grave_robber"
msgid "I shall make do without."
msgstr "Обійдусь і без твоєї допомоги."

msgctxt "affinity_change_bark_add_token_observing_jealous+hellion"
msgid "Jealous of me, are you?!"
msgstr "Заздриш мені?"

msgctxt "affinity_change_bark_add_token_observing_jealous+highwayman"
msgid "Trust me so little, do you?"
msgstr "Зовсім мені не довіряєш?"

msgctxt "affinity_change_bark_add_token_observing_jealous+jester"
msgid "We'll see who's laughing when <i>you</i> need help!"
msgstr "Ти ще благатимеш про <i>допомогу</i>!"

msgctxt "affinity_change_bark_add_token_observing_jealous+leper"
msgid "The jackal stalks alone."
msgstr "У вовка-одинака друзів нема."

msgctxt "affinity_change_bark_add_token_observing_jealous+man_at_arms"
msgid "You wouldn't know good leadership!"
msgstr "Хто ж поведе тебе в бій?"

msgctxt "affinity_change_bark_add_token_observing_jealous+occultist"
msgid "You fear my strength."
msgstr "Ти боїшся моєї сили."

msgctxt "affinity_change_bark_add_token_observing_jealous+plague_doctor"
msgid "Reckless!"
msgstr "Безрозсудність!"

msgctxt "affinity_change_bark_add_token_observing_jealous+runaway"
msgid "Fine! I don't need you!"
msgstr "Ну й гаразд, обійдуся без тебе."

msgctxt "affinity_change_bark_add_token_observing_jealous+vestal"
msgid "It's as though you wish me weak."
msgstr "Так і думала — ти бажаєш мені зла."

msgctxt "affinity_change_bark_add_token_participating@0"
msgid "For you, friend!"
msgstr "За тебе, друже!"

msgctxt "affinity_change_bark_add_token_participating@1"
msgid "Make use of this!"
msgstr "Скористайся цим!"

msgctxt "affinity_change_bark_add_token_participating@2"
msgid "Perhaps you've got an angle I do not."
msgstr "Напевно, ти бачиш щось таке, що я не бачу."

msgctxt "affinity_change_bark_add_token_participating+flagellant"
msgid "A glimpse of the power my pain has bought."
msgstr "Відблиск сили, яку допоміг отримати мій біль."

msgctxt "affinity_change_bark_add_token_participating+grave_robber"
msgid "Try a sip of this. Steadies the hand."
msgstr "Сьорбни — рука міцніше триматиме клинок."

msgctxt "affinity_change_bark_add_token_participating+hellion"
msgid "Flay them from end to end!"
msgstr "Здери з ворога шкіру!"

msgctxt "affinity_change_bark_add_token_participating+highwayman"
msgid "Take your time. Take aim."
msgstr "Не поспішай, добре прицілься."

msgctxt "affinity_change_bark_add_token_participating+jester"
msgid "Look! A weak spot!"
msgstr "О, слабке місце!"

msgctxt "affinity_change_bark_add_token_participating+leper"
msgid "As light parts the fog..."
msgstr "Як світло ріже темряву..."

msgctxt "affinity_change_bark_add_token_participating+man_at_arms"
msgid "Listen here and follow my lead!"
msgstr "Слухай і роби, що я кажу!"

msgctxt "affinity_change_bark_add_token_participating+occultist"
msgid "My power is yours."
msgstr "Моя сила — твоя сила!"

msgctxt "affinity_change_bark_add_token_participating+plague_doctor"
msgid "Allow me to fortify your constitution!"
msgstr "Дозволь зміцнити твоє тіло!"

msgctxt "affinity_change_bark_add_token_participating+runaway"
msgid "See? I ain't all bad."
msgstr "Бачиш, не така я вже й нікчемна."

msgctxt "affinity_change_bark_add_token_participating+vestal"
msgid "There is strength in you. Find it."
msgstr "У тобі є прихована сила, віднайди її."

msgctxt "affinity_change_bark_banter_negative@0"
msgid "Are you mocking me?"
msgstr "Ти що, глузуєш із мене?"

msgctxt "affinity_change_bark_banter_negative@1"
msgid "Can I rely on you out there? Not sure."
msgstr "Покластися на тебе? Навряд."

msgctxt "affinity_change_bark_banter_negative@2"
msgid "Cease that humming, ye gods...!"
msgstr "Досить мекати!"

msgctxt "affinity_change_bark_banter_negative@3"
msgid "Couldn't care less."
msgstr "Мені однаково."

msgctxt "affinity_change_bark_banter_negative@4"
msgid "Don't push your luck."
msgstr "Не випробовуй долю."

msgctxt "affinity_change_bark_banter_negative@5"
msgid "Get out of my face."
msgstr "Геть з моїх очей."

msgctxt "affinity_change_bark_banter_negative@6"
msgid "Get your finger out of your nose."
msgstr "Не зволікай."

msgctxt "affinity_change_bark_banter_negative@7"
msgid "Go bother someone else."
msgstr "Надокучай комусь іншому."

msgctxt "affinity_change_bark_banter_negative@8"
msgid "Grr."
msgstr "Гр-р."

msgctxt "affinity_change_bark_banter_negative@9"
msgid "Hands to yourself."
msgstr "Руки геть."

msgctxt "affinity_change_bark_banter_negative@10"
msgid "How about some peace and quiet?"
msgstr "Як щодо тиші й спокою?"

msgctxt "affinity_change_bark_banter_negative@11"
msgid "I doubt your mettle."
msgstr "Щось не вірю я в твою відвагу."

msgctxt "affinity_change_bark_banter_negative@12"
msgid "I find you grating in the extreme."
msgstr "Уже нема сил тебе терпіти."

msgctxt "affinity_change_bark_banter_negative@13"
msgid "I'd rather sleep by the horses."
msgstr "У компанії коней і то краще."

msgctxt "affinity_change_bark_banter_negative@14"
msgid "I'm trying to sleep!"
msgstr "Я тут заснути намагаюсь!"

msgctxt "affinity_change_bark_banter_negative@15"
msgid "Imbecile!"
msgstr "Безголове створіння!"

msgctxt "affinity_change_bark_banter_negative@16"
msgid "Just back off, will you?"
msgstr "Іди геть."

msgctxt "affinity_change_bark_banter_negative@17"
msgid "Leave me be."
msgstr "Не набридай."

msgctxt "affinity_change_bark_banter_negative@18"
msgid "Let me alone!"
msgstr "Відчепись, кажу!"

msgctxt "affinity_change_bark_banter_negative@19"
msgid "Must you breathe through your mouth?"
msgstr "Тобі обов’язково через рота дихати?"

msgctxt "affinity_change_bark_banter_negative@20"
msgid "Out of my way."
msgstr "Забирайся звідси."

msgctxt "affinity_change_bark_banter_negative@21"
msgid "Something in here reeks. "
msgstr "Смердиш, мов щось здохло. "

msgctxt "affinity_change_bark_banter_negative@22"
msgid "Stop fidgeting!"
msgstr "Досить вовтузитись!"

msgctxt "affinity_change_bark_banter_negative@23"
msgid "Stop picking at yourself!"
msgstr "Не метушись!"

msgctxt "affinity_change_bark_banter_negative@24"
msgid "Stop wasting my time."
msgstr "Не витрачай мій час."

msgctxt "affinity_change_bark_banter_negative@25"
msgid "Stuck on watch with <i>you?</i> Great."
msgstr "У дозорі з <i>тобою?</i> Цього ще не вистачало..."

msgctxt "affinity_change_bark_banter_negative@26"
msgid "This coach is too small and you're too loud."
msgstr "Диліжанс занадто малий, а від тебе забагато шуму."

msgctxt "affinity_change_bark_banter_negative@27"
msgid "This journey is trying enough already."
msgstr "Мені й без того вистачає страждань."

msgctxt "affinity_change_bark_banter_negative@28"
msgid "Those rations were mine!"
msgstr "Це була моя їжа!"

msgctxt "affinity_change_bark_banter_negative@29"
msgid "Ugh. Piss off already."
msgstr "Тьху. Хай тобі грець."

msgctxt "affinity_change_bark_banter_negative@30"
msgid "Wish we'd never picked you up."
msgstr "Бодай би ніколи тебе не зустрічати..."

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative@31"
#| msgid "You're a fool."
msgctxt "affinity_change_bark_banter_negative@31"
msgid "You're a fool."
msgstr "Дурне створіння."

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative@32"
#| msgid "Your very presence offends me."
msgctxt "affinity_change_bark_banter_negative@32"
msgid "Your very presence offends me."
msgstr "Мене нудить від самої твоєї присутності."

msgctxt "affinity_change_bark_banter_negative_high_stress@0"
msgid "All life will soon be lost - especially yours!"
msgstr "Незабаром згине все. Ти в першу чергу!"

msgctxt "affinity_change_bark_banter_negative_high_stress@1"
msgid "Better to die alone - and die we will."
msgstr "Краще померти на самоті. Саме так це і буде."

msgctxt "affinity_change_bark_banter_negative_high_stress@2"
msgid "I have seen the end. You don't make it."
msgstr "Я знаю, чим це скінчиться. Ти не виживеш."

msgctxt "affinity_change_bark_banter_negative_high_stress@3"
msgid "If I <i>were</i> afraid, that's no business of yours."
msgstr "<i>Якщо</i> я й боюся, то це не твоя справа."

msgctxt "affinity_change_bark_banter_negative_high_stress@4"
msgid "If I gut you now, more food for me."
msgstr "Якщо я вб’ю тебе, мені дістанеться більше їжі."

msgctxt "affinity_change_bark_banter_negative_high_stress@5"
msgid "If you're the best we've got, 'tis hopeless."
msgstr "Якщо ти наша надія, то ми приречені."

msgctxt "affinity_change_bark_banter_negative_high_stress@6"
msgid "Loathsome coward!"
msgstr "Яке боягузтво!"

msgctxt "affinity_change_bark_banter_negative_high_stress@7"
msgid "My resolve wanes - leave me."
msgstr "Моя рішучість слабшає... Залиш мене."

msgctxt "affinity_change_bark_banter_negative_high_stress@8"
msgid "Save your bile for our enemies!"
msgstr "Прибережи свої кпини для ворогів!"

msgctxt "affinity_change_bark_banter_negative_high_stress@9"
msgid "Was there no one capable left...?"
msgstr "Невже самі нездари залишилися?"

msgctxt "affinity_change_bark_banter_negative_high_stress@10"
msgid "You wear on my nerves...!"
msgstr "Не дій мені на нерви!"

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative_high_stress@11"
#| msgid "You've led them to our heels!"
msgctxt "affinity_change_bark_banter_negative_high_stress@11"
msgid "You've led them to our heels!"
msgstr "Це завдяки тобі вони наступають нам на п’яти!"

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative_high_stress@12"
#| msgid "Your presence on this coach dooms us!"
msgctxt "affinity_change_bark_banter_negative_high_stress@12"
msgid "Your presence on this coach dooms us!"
msgstr "Ми приречені, тому що на цьому Диліжансі є ти."

msgctxt "affinity_change_bark_banter_negative_high_stress+flagellant@0"
msgid "Defy my sacred calling and feel my wrath!"
msgstr "Кинь виклик моєму священному покликанню і відчуй мій гнів!"

msgctxt "affinity_change_bark_banter_negative_high_stress+flagellant@1"
msgid "I spit in the face of your so-called virtues!"
msgstr "Мені плювати на ваші так звані чесноти!"

msgctxt "affinity_change_bark_banter_negative_high_stress+grave_robber@0"
msgid "Any company but yours, lickspittle."
msgstr "Хто завгодно, аби не ти, підлизо!"

msgctxt "affinity_change_bark_banter_negative_high_stress+grave_robber@1"
msgid "Your attempts at comfort are most patronizing."
msgstr "Твої спроби заспокоїти звучать дуже зверхньо."

msgctxt "affinity_change_bark_banter_negative_high_stress+hellion@0"
msgid "I pity your mother!"
msgstr "Мені шкода твою маму!"

msgctxt "affinity_change_bark_banter_negative_high_stress+hellion@1"
msgid "I'll carve out your tongue!"
msgstr "Я тобі язика відріжу!"

msgctxt "affinity_change_bark_banter_negative_high_stress+highwayman@0"
msgid "Get your boot off my bunk if you value that foot."
msgstr "Забери ноги, якщо вони тобі ще потрібні."

msgctxt "affinity_change_bark_banter_negative_high_stress+highwayman@1"
msgid "What's <i>wrong?</i> Look outside. You daft?"
msgstr "Ти при своєму <i>розумі?</i> Виглянь у вікно."

msgctxt "affinity_change_bark_banter_negative_high_stress+jester@0"
msgid "Entertain yourself. I'm done."
msgstr "Розважай себе самотужки. З мене досить..."

msgctxt "affinity_change_bark_banter_negative_high_stress+jester@1"
msgid "Won't let YOU of all people crash my pity party."
msgstr "Я сумуватиму скільки захочу."

msgctxt "affinity_change_bark_banter_negative_high_stress+leper@0"
msgid "*tense breathing*"
msgstr "*напружено дихає*"

msgctxt "affinity_change_bark_banter_negative_high_stress+leper@1"
msgid "You anger the wrong man."
msgstr "Ти сердиш того, кого краще не сердити."

msgctxt "affinity_change_bark_banter_negative_high_stress+man_at_arms@0"
msgid "Has a man not earned some peace?"
msgstr "Хіба старий не заслужив спокою?"

msgctxt "affinity_change_bark_banter_negative_high_stress+man_at_arms@1"
msgid "Sod off if ye know what's best!"
msgstr "Забирайся, якщо все і так знаєш!"

msgctxt "affinity_change_bark_banter_negative_high_stress+occultist@0"
msgid "Pester some other poor soul."
msgstr "Іди надокучай іншій бідній душі."

msgctxt "affinity_change_bark_banter_negative_high_stress+occultist@1"
msgid "Test me and suffer the consequences."
msgstr "Удар мене, і ти пожалкуєш."

msgctxt "affinity_change_bark_banter_negative_high_stress+plague_doctor@0"
msgid "I've had better company in the morgue!"
msgstr "Та в мене в трупарні була краща компанія."

msgctxt "affinity_change_bark_banter_negative_high_stress+plague_doctor@1"
msgid "You! Reckless! You threaten us all!"
msgstr "Ти не в собі. Ти нас занапастиш!"

msgctxt "affinity_change_bark_banter_negative_high_stress+runaway@0"
msgid "... Not now. Leave me alone."
msgstr "Не зараз. Залиш мене в спокої."

msgctxt "affinity_change_bark_banter_negative_high_stress+runaway@1"
msgid "I'll melt your gloves to your hands!"
msgstr "Не грай із вогнем, бо обпечешся!"

msgctxt "affinity_change_bark_banter_negative_high_stress+vestal@0"
msgid "Away with you. Meditate on your failings."
msgstr "Забирайся геть. Думай про свої невдачі."

msgctxt "affinity_change_bark_banter_negative_high_stress+vestal@1"
msgid "Do I interrupt <i>your</i> prayers?"
msgstr "Хіба я колись заважав <i>твоїм</i> молитвам?"

msgctxt "affinity_change_bark_banter_negative_low_stress@0"
msgid "Do I look as though I'm here to make friends?"
msgstr "Думаєш, мені потрібні друзі?"

msgctxt "affinity_change_bark_banter_negative_low_stress@1"
msgid "I need my rest! "
msgstr "Мені потрібно відпочити! "

msgctxt "affinity_change_bark_banter_negative_low_stress@2"
msgid "I'd pity you, had I pity left."
msgstr "Мені вдалося б тебе пожаліти, якби в мене залишилась хоч крапля жалю."

msgctxt "affinity_change_bark_banter_negative_low_stress@3"
msgid "I'd sooner trust a swine."
msgstr "Та я скоріше повірю змії."

msgctxt "affinity_change_bark_banter_negative_low_stress@4"
msgid "My waterskin! You nabbed it!"
msgstr "Мій міх! Твоїх рук справа?"

msgctxt "affinity_change_bark_banter_negative_low_stress@5"
msgid "Ought to abandon you at the inn."
msgstr "Треба було залишити тебе в трактирі."

msgctxt "affinity_change_bark_banter_negative_low_stress@6"
msgid "Remind me - who asked you?"
msgstr "Нагадай: хто тебе просив це робити?"

msgctxt "affinity_change_bark_banter_negative_low_stress@7"
msgid "Spare me the nattering."
msgstr "Досить уже балаканини."

msgctxt "affinity_change_bark_banter_negative_low_stress@8"
msgid "Surely there are chores that need doing."
msgstr "Візьмись за щось важливіше."

msgctxt "affinity_change_bark_banter_negative_low_stress@9"
msgid "This journey was taxing enough <i>before</i> you."
msgstr "Моя подорож була виснажливою ще <i>до</i> нашої зустрічі."

msgctxt "affinity_change_bark_banter_negative_low_stress@10"
msgid "You are beneath contempt."
msgstr "Ти жалюгідне створіння."

msgctxt "affinity_change_bark_banter_negative_low_stress@11"
msgid "You bring nothing to our table."
msgstr "Від тебе жодної користі."

msgctxt "affinity_change_bark_banter_negative_low_stress@12"
msgid "You're reckless with our stores!"
msgstr "Ти безрозсудно витрачаєш наші припаси."

msgctxt "affinity_change_bark_banter_negative_low_stress+flagellant@0"
msgid "Another unbeliever at my flank..."
msgstr "Ще один невіруючий поряд зі мною..."

msgctxt "affinity_change_bark_banter_negative_low_stress+flagellant@1"
msgid "I must meditate. Begone."
msgstr "Мені потрібно помедитувати. Іди звідси."

msgctxt "affinity_change_bark_banter_negative_low_stress+grave_robber@0"
msgid "I'm reading! Do you even know what that is?"
msgstr "Я читаю! Ти хоч знаєш, як це — читати?"

msgctxt "affinity_change_bark_banter_negative_low_stress+grave_robber@1"
msgid "Off with ye!"
msgstr "Тпрусь!"

msgctxt "affinity_change_bark_banter_negative_low_stress+hellion@0"
msgid "I was saving that for later!"
msgstr "Це було на потім!"

msgctxt "affinity_change_bark_banter_negative_low_stress+hellion@1"
msgid "Like a horse-fly, you are!"
msgstr "Ти чіпляєшся, мов реп’ях!"

msgctxt "affinity_change_bark_banter_negative_low_stress+highwayman@0"
msgid "Leave me be."
msgstr "Не набридай."

msgctxt "affinity_change_bark_banter_negative_low_stress+highwayman@1"
msgid "Tend to your own business."
msgstr "Це тебе не обходить."

msgctxt "affinity_change_bark_banter_negative_low_stress+jester@0"
msgid "Goodness, who let you aboard?"
msgstr "Та хто тебе пустив сюди?"

msgctxt "affinity_change_bark_banter_negative_low_stress+jester@1"
msgid "Oh, you're no fun."
msgstr "Що ж ти за зануда..."

msgctxt "affinity_change_bark_banter_negative_low_stress+leper@0"
msgid "*annoyed huff*"
msgstr "*роздратоване пихкання*"

msgctxt "affinity_change_bark_banter_negative_low_stress+leper@1"
msgid "Is that some insect buzzing beside my ear?"
msgstr "Що це за комарик дзижчить біля мого вуха?"

msgctxt "affinity_change_bark_banter_negative_low_stress+man_at_arms@0"
msgid "I don't question <i>your</i> every decision!"
msgstr "Я ж <i>тебе</i> не допитую!"

msgctxt "affinity_change_bark_banter_negative_low_stress+man_at_arms@1"
msgid "Yer troublesome. Shoo."
msgstr "Геть! Від тебе лише проблеми."

msgctxt "affinity_change_bark_banter_negative_low_stress+occultist@0"
msgid "Be wary of prying, <i>friend.</i>"
msgstr "Цікавість занапастила кішку, <i>друже.</i>"

msgctxt "affinity_change_bark_banter_negative_low_stress+occultist@1"
msgid "Respect my silence, please."
msgstr "Поважай моє право на тишу."

msgctxt "affinity_change_bark_banter_negative_low_stress+plague_doctor@0"
msgid "Don't make an enemy of your physiker!"
msgstr "Не роби ворога з власного лікаря!"

msgctxt "affinity_change_bark_banter_negative_low_stress+plague_doctor@1"
msgid "Leave me to my studies!"
msgstr "Не чіпай мене під час досліджень!"

msgctxt "affinity_change_bark_banter_negative_low_stress+runaway@0"
msgid "Can't say I trust you yet."
msgstr "Не можу сказати, що тобі дуже довіряю."

msgctxt "affinity_change_bark_banter_negative_low_stress+runaway@1"
msgid "Why you wanna know so much about me?"
msgstr "Звідки такий інтерес до мене?"

msgctxt "affinity_change_bark_banter_negative_low_stress+vestal@0"
msgid "I find your hygiene troubling."
msgstr "У тебе якісь негаразди з гігієною."

msgctxt "affinity_change_bark_banter_negative_low_stress+vestal@1"
msgid "You may sit beside me, but be <i>quiet.</i>"
msgstr "Можеш сидіти поряд, але <i>ні пари з вуст.</i>"

msgctxt "affinity_change_bark_banter_negative_med_stress@0"
msgid "Drunkard! Keep your wits about you!"
msgstr "П’янюга, тримай такі жарти при собі!"

msgctxt "affinity_change_bark_banter_negative_med_stress@1"
msgid "Even the beasts find you bothersome."
msgstr "Від тебе вже навіть тварини виють."

msgctxt "affinity_change_bark_banter_negative_med_stress@2"
msgid "Feckless dolt."
msgstr "Дурне створіння."

msgctxt "affinity_change_bark_banter_negative_med_stress@3"
msgid "Keep your sorrows to yourself."
msgstr "Тримай свою журбу при собі."

msgctxt "affinity_change_bark_banter_negative_med_stress@4"
msgid "Lout!"
msgstr "Яке нахабство!"

msgctxt "affinity_change_bark_banter_negative_med_stress@5"
msgid "Our luck took a fell turn when we picked you up."
msgstr "Успіх відвернувся від нас, коли ми тебе підібрали."

msgctxt "affinity_change_bark_banter_negative_med_stress@6"
msgid "That's my bunk! Off!"
msgstr "Це моє ліжко. Геть!"

msgctxt "affinity_change_bark_banter_negative_med_stress@7"
msgid "Trust not a word this one speaks!"
msgstr "Не вірю жодному твоєму слову!"

msgctxt "affinity_change_bark_banter_negative_med_stress@8"
msgid "You cowardly seek-sorrow!"
msgstr "Ти боягузливо шукаєш, хто б тебе пожалів."

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative_med_stress@9"
#| msgid "You're a curse upon this voyage."
msgctxt "affinity_change_bark_banter_negative_med_stress@9"
msgid "You're a curse upon this voyage."
msgstr "Ти прокляття на нашому шляху."

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative_med_stress@10"
#| msgid "You're a rough piece of work."
msgctxt "affinity_change_bark_banter_negative_med_stress@10"
msgid "You're a rough piece of work."
msgstr "Ти нікчема."

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative_med_stress@11"
#| msgid "You're hardly worth the disdain."
msgctxt "affinity_change_bark_banter_negative_med_stress@11"
msgid "You're hardly worth the disdain."
msgstr "Тебе навіть зневажати не виходить."

#, fuzzy
#| msgctxt "affinity_change_bark_banter_negative_med_stress@12"
#| msgid "Your carelessness endangers us!"
msgctxt "affinity_change_bark_banter_negative_med_stress@12"
msgid "Your carelessness endangers us!"
msgstr "Твоя недбалість ставить нас під загрозу!"

msgctxt "affinity_change_bark_banter_negative_med_stress+flagellant@0"
msgid "You flinch from my company! Weakling!"
msgstr "Ти боїшся бути поруч зі мною!"

msgctxt "affinity_change_bark_banter_negative_med_stress+flagellant@1"
msgid "You imperil our chances at victory!"
msgstr "Ти зводиш наші шанси на перемогу до нуля!"

msgctxt "affinity_change_bark_banter_negative_med_stress+grave_robber@0"
msgid "Take your trifling concerns elsewhere."
msgstr "Нікого не турбують твої проблеми."

msgctxt "affinity_change_bark_banter_negative_med_stress+grave_robber@1"
msgid "You remind me of someone I once loathed."
msgstr "Ти нагадуєш мені старого ворога."

msgctxt "affinity_change_bark_banter_negative_med_stress+hellion@0"
msgid "Those who sit beside me must EARN it."
msgstr "Місце поруч зі мною ще треба заслужити."

msgctxt "affinity_change_bark_banter_negative_med_stress+hellion@1"
msgid "You've a coward's stink on you!"
msgstr "Від тебе аж тхне боягузтвом!"

msgctxt "affinity_change_bark_banter_negative_med_stress+highwayman@0"
msgid "Accuse me of cheatin' one more time..."
msgstr "Ще раз скажи, що я тебе підвів..."

msgctxt "affinity_change_bark_banter_negative_med_stress+highwayman@1"
msgid "You'll double-cross me. I'm sure."
msgstr "Ти точно хочеш мене підставити."

msgctxt "affinity_change_bark_banter_negative_med_stress+jester@0"
msgid "You exhaust me."
msgstr "Ти граєш на моїх нервах."

msgctxt "affinity_change_bark_banter_negative_med_stress+jester@1"
msgid "Your presence is a drain on morale."
msgstr "Твоя присутність вимотує нерви."

msgctxt "affinity_change_bark_banter_negative_med_stress+leper@0"
msgid "Allow a tired man his rest."
msgstr "Той, хто стомився, має відпочити."

msgctxt "affinity_change_bark_banter_negative_med_stress+leper@1"
msgid "Begone. I have suffered long already."
msgstr "Геть! Я страждав надто довго."

msgctxt "affinity_change_bark_banter_negative_med_stress+man_at_arms@0"
msgid "Fate has sent you here to torment me."
msgstr "Доля підіслала тебе, щоб я мучився."

msgctxt "affinity_change_bark_banter_negative_med_stress+man_at_arms@1"
msgid "I might be old but I'll still tan yer hide."
msgstr "Якщо не заспокоїшся, тобі буде непереливки."

msgctxt "affinity_change_bark_banter_negative_med_stress+occultist@0"
msgid "*long-suffering sigh*"
msgstr "*довге болісне зітхання*"

msgctxt "affinity_change_bark_banter_negative_med_stress+occultist@1"
msgid "I am a patient man, but I have my limits."
msgstr "Навіть у терпіння є межі."

msgctxt "affinity_change_bark_banter_negative_med_stress+plague_doctor@0"
msgid "Have you no sense? I said BEGONE."
msgstr "Не розумієш? Я кажу — геть!"

msgctxt "affinity_change_bark_banter_negative_med_stress+plague_doctor@1"
msgid "You bicker as a child does!"
msgstr "Ти сперечаєшся як дитина!"

msgctxt "affinity_change_bark_banter_negative_med_stress+runaway@0"
msgid "I regret ever opening up to you."
msgstr "Не треба було тобі довіряти."

msgctxt "affinity_change_bark_banter_negative_med_stress+runaway@1"
msgid "I was better off alone."
msgstr "Мені було краще, коли я була сама."

msgctxt "affinity_change_bark_banter_negative_med_stress+vestal@0"
msgid "Babble at someone else. I'm resting."
msgstr "Базікай із кимось іншим. Я відпочиваю."

msgctxt "affinity_change_bark_banter_negative_med_stress+vestal@1"
msgid "Mistake me not for a friend."
msgstr "Навіть не думай, що я твоя подруга."

msgctxt "affinity_change_bark_banter_positive@0"
msgid "At least the company is good."
msgstr "Хоч компанія гарна."

msgctxt "affinity_change_bark_banter_positive@1"
msgid "At least WE get along!"
msgstr "Ми принаймні добре розуміємося."

msgctxt "affinity_change_bark_banter_positive@2"
msgid "At least you don't snore."
msgstr "Не хропиш і добре."

msgctxt "affinity_change_bark_banter_positive@3"
msgid "Can't imagine this trip without you."
msgstr "Не уявляю цю подорож без тебе."

msgctxt "affinity_change_bark_banter_positive@4"
msgid "Chin up."
msgstr "Вище ніс."

msgctxt "affinity_change_bark_banter_positive@5"
msgid "Hah! Tell us another!"
msgstr "Ха-ха, давай ще!"

msgctxt "affinity_change_bark_banter_positive@6"
msgid "Hmm, not bad. Not bad."
msgstr "Непогано, зовсім непогано."

msgctxt "affinity_change_bark_banter_positive@7"
msgid "I am comforted you're near."
msgstr "Мені спокійніше, коли ти поряд."

msgctxt "affinity_change_bark_banter_positive@8"
msgid "I appreciate your candor."
msgstr "Мені подобається твоя відвертість."

msgctxt "affinity_change_bark_banter_positive@9"
msgid "I like your style."
msgstr "Мені подобається твій стиль."

msgctxt "affinity_change_bark_banter_positive@10"
msgid "I'm glad you chose to ride with us."
msgstr "Добре, що ти їдеш із нами."

msgctxt "affinity_change_bark_banter_positive@11"
msgid "I've got your back out there."
msgstr "Можеш розраховувати на мене."

msgctxt "affinity_change_bark_banter_positive@12"
msgid "I've had worse company."
msgstr "Зустрічалися і гірші компаньйони."

msgctxt "affinity_change_bark_banter_positive@13"
msgid "Let's take the first watch together."
msgstr "Давай підемо на першу варту разом."

msgctxt "affinity_change_bark_banter_positive@14"
msgid "Looking good."
msgstr "Добре виглядаєш."

msgctxt "affinity_change_bark_banter_positive@15"
msgid "Pass that waterskin? Thank you..."
msgstr "Передай-но міх. Дякую..."

msgctxt "affinity_change_bark_banter_positive@16"
msgid "Perhaps later we can get a drink?"
msgstr "Ну що, потім вип’ємо по сто?"

msgctxt "affinity_change_bark_banter_positive@17"
msgid "Perhaps the tide is turning."
msgstr "Може, удача ще нам посміхнеться."

msgctxt "affinity_change_bark_banter_positive@18"
msgid "Real glad we ran into you on the road."
msgstr "Добре, що ми зустрілися в дорозі."

msgctxt "affinity_change_bark_banter_positive@19"
msgid "Relax. You're among friends."
msgstr "Розслабся, ти серед друзів."

msgctxt "affinity_change_bark_banter_positive@20"
msgid "Sit next to me at the Inn, won't you?"
msgstr "Сядемо поряд у трактирі?"

msgctxt "affinity_change_bark_banter_positive@21"
msgid "Tell me about yourself..."
msgstr "Розкажи про себе..."

msgctxt "affinity_change_bark_banter_positive@22"
msgid "Things were so boring before you got here."
msgstr "Без тебе було так нудно."

msgctxt "affinity_change_bark_banter_positive@23"
msgid "We'll get through this. Together."
msgstr "Ми впораємося... разом."

msgctxt "affinity_change_bark_banter_positive@24"
msgid "We're lucky to have you."
msgstr "Як добре, що ти з нами!"

msgctxt "affinity_change_bark_banter_positive@25"
msgid "We've survived worse..."
msgstr "Бувало й гірше..."

msgctxt "affinity_change_bark_banter_positive@26"
msgid "You can have the bunk next to mine."
msgstr "Можемо лягти поряд."

msgctxt "affinity_change_bark_banter_positive@27"
msgid "You carry yourself well."
msgstr "Ти добре тримаєшся."

msgctxt "affinity_change_bark_banter_positive@28"
msgid "You give us an edge out there."
msgstr "Ти наш порятунок."

msgctxt "affinity_change_bark_banter_positive@29"
msgid "You improve our odds out there."
msgstr "З тобою в нас більше шансів."

msgctxt "affinity_change_bark_banter_positive@30"
msgid "You make this journey more bearable."
msgstr "З тобою наша подорож трохи легша."

msgctxt "affinity_change_bark_banter_positive@31"
msgid "You're an important part of our little band."
msgstr "З тобою ми сильніші."

msgctxt "affinity_change_bark_banter_positive_high_stress@0"
msgid "A drink to take the edge off?"
msgstr "Хочеш випити, щоб скинути напругу?"

msgctxt "affinity_change_bark_banter_positive_high_stress@1"
msgid "Don't look out. Look at me."
msgstr "Не дивись у вікно. Дивись на мене."

msgctxt "affinity_change_bark_banter_positive_high_stress@2"
msgid "How quickly you've grown on me."
msgstr "Як швидко ти заволоділа моїми почуттями..."

msgctxt "affinity_change_bark_banter_positive_high_stress@3"
msgid "If we go, we go together."
msgstr "Якщо ми вистоїмо, то разом."

msgctxt "affinity_change_bark_banter_positive_high_stress@4"
msgid "Keep to the light. We'll persevere."
msgstr "Тримайся ближче до світла. Ми виберемося."

msgctxt "affinity_change_bark_banter_positive_high_stress@5"
msgid "Lean on me if you require it."
msgstr "Розраховуй на мене, якщо треба буде."

msgctxt "affinity_change_bark_banter_positive_high_stress@6"
msgid "Pleasant company, if ye be my last."
msgstr "Приємна компанія наостанок."

msgctxt "affinity_change_bark_banter_positive_high_stress@7"
msgid "Retreat is not an option."
msgstr "Про відступ навіть не думаємо."

msgctxt "affinity_change_bark_banter_positive_high_stress@8"
msgid "The days blur into one - but at least you're here."
msgstr "Дні зливаються в один, але принаймні ти тут."

msgctxt "affinity_change_bark_banter_positive_high_stress@9"
msgid "Together, we can prevail."
msgstr "Разом ми все подолаємо."

msgctxt "affinity_change_bark_banter_positive_high_stress@10"
msgid "You inspire me to be better."
msgstr "Ти надихаєш мене стати краще."

msgctxt "affinity_change_bark_banter_positive_high_stress@11"
msgid "You lift my spirits when they most need it."
msgstr "Ти піднімаєш мій настрій саме тоді, коли це потрібно."

msgctxt "affinity_change_bark_banter_positive_high_stress@12"
msgid "You're a tough one."
msgstr "А ти міцний горішок."

msgctxt "affinity_change_bark_banter_positive_high_stress+flagellant@0"
msgid "Oh, I have no plans to die. Heh heh heh."
msgstr "О, я не планую помирати, хе-хе-хе."

msgctxt "affinity_change_bark_banter_positive_high_stress+flagellant@1"
msgid "Spirited you are, even in the face of madness!"
msgstr "Ви не втрачаєте духу, навіть коли відбувається божевілля!"

msgctxt "affinity_change_bark_banter_positive_high_stress+grave_robber@0"
msgid "For me? Why... thank you."
msgstr "Це мені? Ого... Дякую."

msgctxt "affinity_change_bark_banter_positive_high_stress+grave_robber@1"
msgid "You're a welcome distraction."
msgstr "З тобою приємно проводити час."

msgctxt "affinity_change_bark_banter_positive_high_stress+hellion@0"
msgid "My birthplace calls to me..."
msgstr "Батьківщина кличе мене..."

msgctxt "affinity_change_bark_banter_positive_high_stress+hellion@1"
msgid "The end bears down. Let us face it!"
msgstr "Кінець уже наближається. Зустріньмо його разом!"

msgctxt "affinity_change_bark_banter_positive_high_stress+highwayman@0"
msgid "Sit with me. I'm... tense."
msgstr "Сядь зі мною. Я собі місця не знайду..."

msgctxt "affinity_change_bark_banter_positive_high_stress+highwayman@1"
msgid "Y'have no idea how much I needed that."
msgstr "Ти навіть не уявляєш, як це було мені потрібно."

msgctxt "affinity_change_bark_banter_positive_high_stress+jester@0"
msgid "... Good one."
msgstr "Гарний жарт."

msgctxt "affinity_change_bark_banter_positive_high_stress+jester@1"
msgid "Clever head on those shoulders."
msgstr "У тебе розумна голова на плечах."

msgctxt "affinity_change_bark_banter_positive_high_stress+leper@0"
msgid "No - you deserve to be here same as I."
msgstr "Ти не заслуговуєш на такі негаразди."

msgctxt "affinity_change_bark_banter_positive_high_stress+leper@1"
msgid "Yours is a soul of worth."
msgstr "У тебе гідна душа."

msgctxt "affinity_change_bark_banter_positive_high_stress+man_at_arms@0"
msgid "Promise me it ain't for naught?"
msgstr "Пообіцяй мені, що із цього буде користь."

msgctxt "affinity_change_bark_banter_positive_high_stress+man_at_arms@1"
msgid "You're right - I shouldn't fret."
msgstr "Саме так. Мені не варто турбуватися."

msgctxt "affinity_change_bark_banter_positive_high_stress+occultist@0"
msgid "Let us tend the torch together."
msgstr "Давай разом нести смолоскип."

msgctxt "affinity_change_bark_banter_positive_high_stress+occultist@1"
msgid "What's that? I was being \"morose\"?"
msgstr "Що ти кажеш? Я виглядаю сумним?"

msgctxt "affinity_change_bark_banter_positive_high_stress+plague_doctor@0"
msgid "Here - you've got splatter on you."
msgstr "Дивись, на тобі пляма."

msgctxt "affinity_change_bark_banter_positive_high_stress+plague_doctor@1"
msgid "You have taught me much."
msgstr "Я багато чого в тебе навчилася."

msgctxt "affinity_change_bark_banter_positive_high_stress+runaway@0"
msgid "How do you do it? Keep fighting?"
msgstr "Як тобі вдається боротися?"

msgctxt "affinity_change_bark_banter_positive_high_stress+runaway@1"
msgid "Sorry for snappin' at ya."
msgstr "Вибач, що зірвалася на тебе."

msgctxt "affinity_change_bark_banter_positive_high_stress+vestal@0"
msgid "... You shouldn't have. Light keep you."
msgstr "Не варто було... Борони тебе Світло."

msgctxt "affinity_change_bark_banter_positive_high_stress+vestal@1"
msgid "Some days I fear we are on the losing side..."
msgstr "Іноді я боюся, що ми не впораємося..."

msgctxt "affinity_change_bark_banter_positive_low_stress@0"
msgid "A bleak road it would be for your absence."
msgstr "Якби тебе не було тут, дорога була б безрадісною."

msgctxt "affinity_change_bark_banter_positive_low_stress@1"
msgid "Another round?"
msgstr "Ще по одній?"

msgctxt "affinity_change_bark_banter_positive_low_stress@2"
msgid "I dare say, you lend me hope."
msgstr "Ти даєш мені надію."

msgctxt "affinity_change_bark_banter_positive_low_stress@3"
msgid "I find your candor most refreshing."
msgstr "Твоя відвертість освіжає як ніщо інше."

msgctxt "affinity_change_bark_banter_positive_low_stress@4"
msgid "I never tire of your stories."
msgstr "Твої історії ніколи не набридають."

msgctxt "affinity_change_bark_banter_positive_low_stress@5"
msgid "Let us take watch together."
msgstr "Давай підемо на варту разом."

msgctxt "affinity_change_bark_banter_positive_low_stress@6"
msgid "Take a sip of wine?"
msgstr "Ковтнеш вина?"

msgctxt "affinity_change_bark_banter_positive_low_stress@7"
msgid "The worse it gets, the more we'll need each other."
msgstr "Що гірше стає, то потрібніші ми одне одному."

msgctxt "affinity_change_bark_banter_positive_low_stress@8"
msgid "We're better off in your company."
msgstr "Разом із тобою краще."

msgctxt "affinity_change_bark_banter_positive_low_stress@9"
msgid "You just might be the edge we need."
msgstr "Ти та соломинка, яка схилить шальки терезів на нашу користь."

msgctxt "affinity_change_bark_banter_positive_low_stress@10"
msgid "You've got something the others don't."
msgstr "У тебе є те, чого немає в інших."

msgctxt "affinity_change_bark_banter_positive_low_stress@11"
msgid "You've no idea how welcome a change you are."
msgstr "Ти навіть не уявляєш, як нам потрібні були зміни."

msgctxt "affinity_change_bark_banter_positive_low_stress+flagellant@0"
msgid "I sense you are meant for glory."
msgstr "Я відчуваю, що на тебе чекає слава."

msgctxt "affinity_change_bark_banter_positive_low_stress+flagellant@1"
msgid "Pass me that vial of holy vinegar, if you would."
msgstr "Передай, будь ласка, мені цей флакон зі святим оцтом."

msgctxt "affinity_change_bark_banter_positive_low_stress+grave_robber@0"
msgid "Share a flask? I'm parched."
msgstr "Поділишся фляжкою? Умираю від спраги."

msgctxt "affinity_change_bark_banter_positive_low_stress+grave_robber@1"
msgid "They broke the mold with you."
msgstr "Ти незвичайна людина."

msgctxt "affinity_change_bark_banter_positive_low_stress+hellion@0"
msgid "Born under the right moon, you were!"
msgstr "Твоє народження відбулося під щасливою зіркою."

msgctxt "affinity_change_bark_banter_positive_low_stress+hellion@1"
msgid "The horses like you. A good omen."
msgstr "Ти подобаєшся коням. Це хороший знак."

msgctxt "affinity_change_bark_banter_positive_low_stress+highwayman@0"
msgid "Heh. Ain't bad, your jokes."
msgstr "Хе-хе, гарне в тебе почуття гумору."

msgctxt "affinity_change_bark_banter_positive_low_stress+highwayman@1"
msgid "Think I have some dice rattling around in here..."
msgstr "Мені почулося, що десь тут котяться гральні кості..."

msgctxt "affinity_change_bark_banter_positive_low_stress+jester@0"
msgid "Heh heh. Good one."
msgstr "Хе-хе... Непогано."

msgctxt "affinity_change_bark_banter_positive_low_stress+jester@1"
msgid "Tell you 'bout my travels? Suppose I can."
msgstr "Розповісти про мої мандри? Можу."

msgctxt "affinity_change_bark_banter_positive_low_stress+leper@0"
msgid "My story? I suppose, for you..."
msgstr "Розповісти про себе? Ну, якщо просиш..."

msgctxt "affinity_change_bark_banter_positive_low_stress+leper@1"
msgid "You are a rock in troublesome times."
msgstr "У скрутні часи ти як скеля."

msgctxt "affinity_change_bark_banter_positive_low_stress+man_at_arms@0"
msgid "Another story? Glad you asked..."
msgstr "Ще історію? Радий, що сподобалось..."

msgctxt "affinity_change_bark_banter_positive_low_stress+man_at_arms@1"
msgid "Pass that here. I'll mend it."
msgstr "Дай мені. Я полагоджу."

msgctxt "affinity_change_bark_banter_positive_low_stress+occultist@0"
msgid "Pass the snuff box, would you?"
msgstr "Давай-но сюди табакерку."

msgctxt "affinity_change_bark_banter_positive_low_stress+occultist@1"
msgid "You tell such intriguing stories."
msgstr "Твої історії такі захоплюючі!"

msgctxt "affinity_change_bark_banter_positive_low_stress+plague_doctor@0"
msgid "At last! Someone I can work with!"
msgstr "Нарешті знайшлася людина, з якою приємно працювати!"

msgctxt "affinity_change_bark_banter_positive_low_stress+plague_doctor@1"
msgid "Think of all we can teach these people!"
msgstr "Подумай, чого ми можемо їх навчити!"

msgctxt "affinity_change_bark_banter_positive_low_stress+runaway@0"
msgid "I like you when you're quiet."
msgstr "Мені з тобою непогано, коли ти мовчиш."

msgctxt "affinity_change_bark_banter_positive_low_stress+runaway@1"
msgid "You ain't so bad."
msgstr "А ти нічого так."

msgctxt "affinity_change_bark_banter_positive_low_stress+vestal@0"
msgid "Maintain your calm. I can help."
msgstr "Зберігай спокій. Я допоможу."

msgctxt "affinity_change_bark_banter_positive_low_stress+vestal@1"
msgid "Sit and read with me?"
msgstr "Сядеш почитати зі мною?"

msgctxt "affinity_change_bark_banter_positive_med_stress@0"
msgid "A game? Something to distract my mind?"
msgstr "Зіграти? Щоб відволіктись на щось?"

msgctxt "affinity_change_bark_banter_positive_med_stress@1"
msgid "Be vigilant. I'll sit up with you."
msgstr "Будь напоготові — я сидітиму поряд із тобою."

msgctxt "affinity_change_bark_banter_positive_med_stress@2"
msgid "I find myself warming to you."
msgstr "Ти починаєш мені подобатися."

msgctxt "affinity_change_bark_banter_positive_med_stress@3"
msgid "I trust you not to disappoint me."
msgstr "Я вірю, що ти мене не розчаруєш."

msgctxt "affinity_change_bark_banter_positive_med_stress@4"
msgid "I would rather not sleep alone..."
msgstr "Спати на самоті — не те, чого душа бажає..."

msgctxt "affinity_change_bark_banter_positive_med_stress@5"
msgid "If you have need of me, simply ask."
msgstr "Якщо знадоблюся, просто поклич."

msgctxt "affinity_change_bark_banter_positive_med_stress@6"
msgid "Let us hope this old thing holds together."
msgstr "Будемо сподіватися, що цей антикваріат витримає."

msgctxt "affinity_change_bark_banter_positive_med_stress@7"
msgid "Let us relax together at the inn..."
msgstr "Давай разом відпочинемо в трактирі..."

msgctxt "affinity_change_bark_banter_positive_med_stress@8"
msgid "Tell me it's going to be all right?"
msgstr "Скажи мені, що все буде добре."

msgctxt "affinity_change_bark_banter_positive_med_stress@9"
msgid "Tell me, are your dreams troubled as mine?"
msgstr "Скажи, твої сни такі само неспокійні, як і мої?"

msgctxt "affinity_change_bark_banter_positive_med_stress@10"
msgid "Truly, you believe this fight is winnable?"
msgstr "Ти віриш, що в цьому бою вдасться перемогти?"

msgctxt "affinity_change_bark_banter_positive_med_stress@11"
msgid "We are not as doomed as I feel, no?"
msgstr "Усе не так погано, як мені здається. Правда?"

msgctxt "affinity_change_bark_banter_positive_med_stress@12"
msgid "We'll look out for one another."
msgstr "Ми дбатимемо про одне одного."

msgctxt "affinity_change_bark_banter_positive_med_stress+flagellant@0"
msgid "I would hear your confession."
msgstr "Я хотів би почути твою сповідь."

msgctxt "affinity_change_bark_banter_positive_med_stress+flagellant@1"
msgid "Your resilience intrigues me."
msgstr "Твоя стійкість дуже цікавить мене."

msgctxt "affinity_change_bark_banter_positive_med_stress+grave_robber@0"
msgid "Spare a conversation? It's... too quiet."
msgstr "Поговоримо? А то якось... занадто тихо."

msgctxt "affinity_change_bark_banter_positive_med_stress+grave_robber@1"
msgid "You speak so certainly of a future..."
msgstr "Ти так упевнено говориш про майбутнє..."

msgctxt "affinity_change_bark_banter_positive_med_stress+hellion@0"
msgid "Cook with me! Share the spoils!"
msgstr "Давай готувати разом! Що залишиться — поділимо."

msgctxt "affinity_change_bark_banter_positive_med_stress+hellion@1"
msgid "Let me paint your face!"
msgstr "Давай я розфарбую тобі обличчя."

msgctxt "affinity_change_bark_banter_positive_med_stress+highwayman@0"
msgid "I expected to die alone. Not... this."
msgstr "Я думав, що помру на самоті. Але тепер..."

msgctxt "affinity_change_bark_banter_positive_med_stress+highwayman@1"
msgid "You're a bright spot."
msgstr "Ти мов сонце, що розганяє хмари..."

msgctxt "affinity_change_bark_banter_positive_med_stress+jester@0"
msgid "Can you carry a tune?"
msgstr "Умієш співати?"

msgctxt "affinity_change_bark_banter_positive_med_stress+jester@1"
msgid "You're a good egg."
msgstr "Ти молодець."

msgctxt "affinity_change_bark_banter_positive_med_stress+leper@0"
msgid "Help me with my wounds...?"
msgstr "Допоможеш обробити рани?"

msgctxt "affinity_change_bark_banter_positive_med_stress+leper@1"
msgid "Your kindness is a rare commodity."
msgstr "Така доброта нечасто зустрічається."

msgctxt "affinity_change_bark_banter_positive_med_stress+man_at_arms@0"
msgid "Glad to have you toiling at my side."
msgstr "Як добре, що ми на одному боці."

msgctxt "affinity_change_bark_banter_positive_med_stress+man_at_arms@1"
msgid "Imagine how lost we'd be without ye."
msgstr "Не знаю, що б ми й робили без тебе."

msgctxt "affinity_change_bark_banter_positive_med_stress+occultist@0"
msgid "Been some time since I had a friend."
msgstr "Уже й не пам’ятаю, коли востаннє я з кимось дружив."

msgctxt "affinity_change_bark_banter_positive_med_stress+occultist@1"
msgid "You're an old soul."
msgstr "У тебе добра душа."

msgctxt "affinity_change_bark_banter_positive_med_stress+plague_doctor@0"
msgid "Do not let me grow morbid..."
msgstr "Не дай мені зневіритися..."

msgctxt "affinity_change_bark_banter_positive_med_stress+plague_doctor@1"
msgid "My mind is troubled. Read with me?"
msgstr "Мене турбують важкі думки. Почитаєш мені?"

msgctxt "affinity_change_bark_banter_positive_med_stress+runaway@0"
msgid "Couldn't do this without ya."
msgstr "Без тебе ми б не впоралися."

msgctxt "affinity_change_bark_banter_positive_med_stress+runaway@1"
msgid "It ain't been long, but you've made a difference."
msgstr "Минуло зовсім небагато часу, але тобі вдалося змінити ситуацію."

msgctxt "affinity_change_bark_banter_positive_med_stress+vestal@0"
msgid "I feel it, yes - but we cannot give in."
msgstr "Я також це відчуваю, але ми не можемо здатися."

msgctxt "affinity_change_bark_banter_positive_med_stress+vestal@1"
msgid "Stand firm! Resist the dark!"
msgstr "Стій непохитно! Чини опір пітьмі!"

msgctxt "affinity_change_bark_feed_jealous@0"
msgid "A delectable gift from the gods, but for ME!"
msgstr "Чудовий дар богів, але він мав бути мій!"

msgctxt "affinity_change_bark_feed_jealous@1"
msgid "Get your paws off <i>my feast!</i>"
msgstr "Лапи геть від <i>мого бенкету!</i>"

msgctxt "affinity_change_bark_feed_jealous@2"
msgid "That banquet is not for you!"
msgstr "Цей бенкет не для тебе!"

msgctxt "affinity_change_bark_feed_jealous@3"
msgid "You gorge yourself on food that should be MINE!"
msgstr "Ти жереш те, що призначено для мене!"

msgctxt "affinity_change_bark_feed_jealous+flagellant"
msgid "How I've bled, and you'd STARVE me...!"
msgstr "Я кров’ю спливаю, а мене голодом морять!"

msgctxt "affinity_change_bark_feed_jealous+grave_robber"
msgid "And I thought I was a glutton."
msgstr "А я ще думала, що це я ненажера."

msgctxt "affinity_change_bark_feed_jealous+hellion"
msgid "I'll sever your greedy hands!"
msgstr "Та я тобі твої жадібні рученята повідриваю!"

msgctxt "affinity_change_bark_feed_jealous+highwayman"
msgid "You have no idea how I've starved..."
msgstr "Ти й гадки не маєш, як я зголоднів..."

msgctxt "affinity_change_bark_feed_jealous+jester"
msgid "You don't get to take this from me!"
msgstr "Ти не можеш забрати це в мене!"

msgctxt "affinity_change_bark_feed_jealous+leper"
msgid "My constitution wanes..."
msgstr "Моє тіло слабшає..."

msgctxt "affinity_change_bark_feed_jealous+man_at_arms"
msgid "I require that! For my strength!"
msgstr "Це мені потрібне, щоб повернути сили!"

msgctxt "affinity_change_bark_feed_jealous+occultist"
msgid "I have hunger beyond your ken."
msgstr "Я такий голодний, що ти навіть уявити не можеш!"

msgctxt "affinity_change_bark_feed_jealous+plague_doctor"
msgid "I require nourishment!"
msgstr "Моєму тілу потрібна їжа!"

msgctxt "affinity_change_bark_feed_jealous+runaway"
msgid "Don't leave me your damned scraps!"
msgstr "Не залишай мені самі недоїдки!"

msgctxt "affinity_change_bark_feed_jealous+vestal"
msgid "This feast is a blessing, you selfish lout!"
msgstr "Цей бенкет — дар нам усім!"

msgctxt "affinity_change_bark_follow_up_hit@0"
msgid "As one!"
msgstr "Нумо разом!"

msgctxt "affinity_change_bark_follow_up_hit@1"
msgid "We're beating them back!"
msgstr "Вони нас не подужають!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget@0"
msgid "They underestimate us!"
msgstr "Нас недооцінили!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget@1"
msgid "We shall be their undoing!"
msgstr "Ми вкоротимо їм віку!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+flagellant"
msgid "Stray not - our burden is divine!"
msgstr "Нам не можна збитися зі шляху істинного: ми несемо божественну ношу!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+grave_robber"
msgid "Brilliant!"
msgstr "Блискуче!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+hellion"
msgid "STRIKE WHERE THEY ALREADY BLEED!"
msgstr "БИЙ У РАНУ!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+highwayman"
msgid "On 'em like a pack of dogs!"
msgstr "Порвемо їх, мов собаки зайця!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+jester"
msgid "I trained as a soloist, but..."
msgstr "Я завжди був одинаком, але..."

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+leper"
msgid "Many are the hands that dig their graves."
msgstr "Ворог уже багатьом поперек горла."

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+man_at_arms"
msgid "This is what we trained for!"
msgstr "До цього ми й готувались."

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+occultist"
msgid "They'll be feeling that one."
msgstr "Ворог це відчує."

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+plague_doctor"
msgid "We weary them!"
msgstr "Виснажимо ворога!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+runaway"
msgid "Hah! Not so fast!"
msgstr "Хе-хе! Не так швидко!"

msgctxt "affinity_change_bark_follow_up_hit_comboedtarget+vestal"
msgid "Light, steer our blades true!"
msgstr "Світло, спрямуй наші клинки точно в ціль!"

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget@0"
msgid "Few more like that and they're done for!"
msgstr "Ще кілька ударів — і все скінчено."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget@1"
msgid "Hah!"
msgstr "Ха!"

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+flagellant"
msgid "Through their flesh, our path to divinity!"
msgstr "Наш шлях — через їхню плоть до божественності!"

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+grave_robber"
msgid "You're dreadful good at this."
msgstr "Одразу видно руку майстра."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+hellion"
msgid "GRAAAAAH!"
msgstr "ГРА-А-А!"

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+highwayman"
msgid "I line 'em up..."
msgstr "Я все зроблю, що треба..."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+jester"
msgid "On three..."
msgstr "На рахунок три..."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+leper"
msgid "As the stream smooths the stone."
msgstr "Як вода точить камінь."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+man_at_arms"
msgid "Warmed the buggers up for you!"
msgstr "Ворог досяг потрібної кондиції."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+occultist"
msgid "I admire your cunning."
msgstr "Твоя хитрість неперевершена."

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+plague_doctor"
msgid "Masterful!"
msgstr "Яка майстерність!"

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+runaway"
msgid "Don't let a single one flee!"
msgstr "Не дай нікому втекти!"

msgctxt "affinity_change_bark_follow_up_hit_crittedtarget+vestal"
msgid "They fear the glare of our torch!"
msgstr "Світло смолоскипа — головний страх ворога."

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget@0"
msgid "Cut them down to size!"
msgstr "Відріжте їм усе зайве!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget@1"
msgid "They'll be scared of US soon!"
msgstr "Скоро ворог тремтітиме від жаху перед нами!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+flagellant"
msgid "They shrivel in the face of my purity!"
msgstr "Їх судомить від моєї чистоти!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+grave_robber"
msgid "Hammer on the nail, you are."
msgstr "Те що треба!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+hellion"
msgid "Never saw you coming!"
msgstr "Я й не чекала."

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+highwayman"
msgid "Now we're fightin' dirty!"
msgstr "Не треба гребувати брудними прийомами!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+jester"
msgid "Got one over on 'em!!"
msgstr "Ми вас переграємо, ми вас знищимо!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+leper"
msgid "The tide turns."
msgstr "Бойова ситуація змінюється."

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+man_at_arms"
msgid "Now! On the offensive!"
msgstr "Ну ж бо, в атаку!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+occultist"
msgid "Now we know what weakens them..."
msgstr "Тепер ми знаємо, у чому слабкість ворога."

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+plague_doctor"
msgid "They are enfeebled! Now!"
msgstr "Ворог слабшає, уперед!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+runaway"
msgid "Yes! Now, again!"
msgstr "Ще раз, так тримати!"

msgctxt "affinity_change_bark_follow_up_hit_debuffedtarget+vestal"
msgid "Banish them!"
msgstr "Знищимо їх!"

msgctxt "affinity_change_bark_follow_up_hit+flagellant"
msgid "Through suffering, strength!"
msgstr "Крізь страждання до сили!"

msgctxt "affinity_change_bark_follow_up_hit+grave_robber"
msgid "I'm charmed."
msgstr "Я в захваті."

msgctxt "affinity_change_bark_follow_up_hit+hellion"
msgid "Chase them back to their dens!"
msgstr "Женіть ворога до самого лігва!"

msgctxt "affinity_change_bark_follow_up_hit+highwayman"
msgid "That's how it's done."
msgstr "Ось як це робиться."

msgctxt "affinity_change_bark_follow_up_hit+jester"
msgid "What a pair, you and I."
msgstr "Ми чудові напарники!"

msgctxt "affinity_change_bark_follow_up_hit+leper"
msgid "None of us fights alone."
msgstr "Ми завжди б’ємося разом."

msgctxt "affinity_change_bark_follow_up_hit+man_at_arms"
msgid "See? We fight as a unit!"
msgstr "Бачиш? Ми — команда!"

msgctxt "affinity_change_bark_follow_up_hit+occultist"
msgid "Snuff them out!"
msgstr "Дамо ворогу прикурити!"

msgctxt "affinity_change_bark_follow_up_hit+plague_doctor"
msgid "A synchronized dissection!"
msgstr "А тепер разом!"

msgctxt "affinity_change_bark_follow_up_hit+runaway"
msgid "Send 'em home limping!"
msgstr "Женімо ворога туди, звідки він з’явився!"

msgctxt "affinity_change_bark_follow_up_hit+vestal"
msgid "Back to the void which birthed ye!"
msgstr "Повертайся в безодню, що породила тебе!"

msgctxt "affinity_change_bark_healing_observing_jealous@0"
msgid "And what of <i>my</i> needs?"
msgstr "А як щодо <i>моїх</i> потреб?"

msgctxt "affinity_change_bark_healing_observing_jealous@1"
msgid "Are my wounds not enough?"
msgstr "Мої рани недостатньо глибокі?"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded@0"
msgid "Do you not see the bloodstains?!"
msgstr "Ти що, не бачиш цих кривавих плям?"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded@1"
msgid "How dare you!"
msgstr "Та як ти смієш?"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+flagellant"
msgid "You think me blighted, beyond hope!"
msgstr "Ти думаєш, що я зіпсований, безнадійний?"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+grave_robber"
msgid "No help coming from you, I see."
msgstr "Від тебе допомоги не дочекаєшся, зрозуміло."

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+hellion"
msgid "You aid our enemies by ignoring me!"
msgstr "Ігноруєш мене — допомагаєш ворогові!"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+highwayman"
msgid "Written me off so soon?"
msgstr "То мене вже скинули з рахунку. Так швидко?"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+jester"
msgid "You'll see no aid from me, either."
msgstr "Ти теж не дочекаєшся від мене допомоги."

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+leper"
msgid "To do nothing is to make a statement."
msgstr "Нічого не робити — це також вчинок."

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+man_at_arms"
msgid "Our campaign shall bear the cost of this foolishness!"
msgstr "Ця дурість може загрожувати нашій місії!"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+occultist"
msgid "A traitor in our midst, perhaps."
msgstr "Не виключено, що серед нас є зрадник."

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+plague_doctor"
msgid "Your eyes must be afflicted!"
msgstr "У тебе щось з очима?"

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+runaway"
msgid "... I thought you were different."
msgstr "Я була про тебе кращої думки..."

msgctxt "affinity_change_bark_healing_observing_jealous_wounded+vestal"
msgid "Repaid for my loyalty with <i>this?</i>"
msgstr "<i>Це</i> відплата за мою відданість?"

msgctxt "affinity_change_bark_healing_observing_jealous+flagellant"
msgid "You see not the greater path before us!"
msgstr "Ти не бачиш великого шляху, що відкривається нам!"

msgctxt "affinity_change_bark_healing_observing_jealous+grave_robber"
msgid "Discard my trust to the midden, would ye?"
msgstr "Моя довіра для тебе зовсім нічого не значить?"

msgctxt "affinity_change_bark_healing_observing_jealous+hellion"
msgid "FORSAKEN!"
msgstr "МЕНЕ КИНУЛИ!"

msgctxt "affinity_change_bark_healing_observing_jealous+highwayman"
msgid "You think me unworthy?"
msgstr "Думаєш, я не вартий допомоги?"

msgctxt "affinity_change_bark_healing_observing_jealous+jester"
msgid "I mistook you."
msgstr "Я помилявся в тобі."

msgctxt "affinity_change_bark_healing_observing_jealous+leper"
msgid "*shakes head*"
msgstr "*хитає головою*"

msgctxt "affinity_change_bark_healing_observing_jealous+man_at_arms"
msgid "You never did hold any respect fer me."
msgstr "У тобі нема ані краплі поваги до мене."

msgctxt "affinity_change_bark_healing_observing_jealous+occultist"
msgid "I am slow to anger, but..."
msgstr "Я дуже терплячий, але..."

msgctxt "affinity_change_bark_healing_observing_jealous+plague_doctor"
msgid "You leave your own medic to suffer!"
msgstr "Ти кидаєш свого ж лікаря напризволяще!"

msgctxt "affinity_change_bark_healing_observing_jealous+runaway"
msgid "That was your last chance!"
msgstr "Це був твій останній шанс!"

msgctxt "affinity_change_bark_healing_observing_jealous+vestal"
msgid "You spurn me in my time of need!"
msgstr "Ти відвертаєшся від мене в скрутну годину!"

msgctxt "affinity_change_bark_healing_observing_mortally_jealous@0"
msgid "Alone at the end..."
msgstr "Під кінець... нікого поруч."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous@1"
msgid "Have I failed you somehow?"
msgstr "Я щось роблю не так?"

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+flagellant"
msgid "My consecrated flesh! My calling! Damn you!"
msgstr "Моя освячена плоть! Моє покликання! Прокляття!"

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+grave_robber"
msgid "I'm not enough, then? So be it."
msgstr "Тож мене замало? То й нехай."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+hellion"
msgid "ROT, THEN!"
msgstr "ХАЙ ТИ ТУТ ХОЧ ЗГНИЄШ!"

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+highwayman"
msgid "Never trusted you a whit."
msgstr "Ніколи тобі не довіряв."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+jester"
msgid "I'll carry on bleeding, then."
msgstr "Добре, я і далі стікатиму кров’ю."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+leper"
msgid "The king on his side upon the board..."
msgstr "Ти підставляєш короля під удар..."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+man_at_arms"
msgid "Leave the sick an' the old behind, is it?"
msgstr "Тобі байдуже на хворих і старих, правда?"

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+occultist"
msgid "Toying with our fates, I see."
msgstr "Ти граєш із нашою долею."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+plague_doctor"
msgid "A poor distribution of aid!"
msgstr "Ти не знаєш, кому треба допомагати."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+runaway"
msgid "My gut told me right about you."
msgstr "Інтуїція мене не підвела щодо тебе."

msgctxt "affinity_change_bark_healing_observing_mortally_jealous+vestal"
msgid "Who will pray for you when I am gone?"
msgstr "Хто молитиметься про тебе, коли мене не стане?"

msgctxt "affinity_change_bark_healing_participating@0"
msgid "Back in the fray with you!"
msgstr "Повертайся до всіх."

msgctxt "affinity_change_bark_healing_participating@1"
msgid "You'll survive this!"
msgstr "Ти витримаєш!"

msgctxt "affinity_change_bark_healing_participating_dd@0"
msgid "Not if I have a say in it!"
msgstr "Ні, я цього не допущу."

msgctxt "affinity_change_bark_healing_participating_dd@1"
msgid "Surrender not!"
msgstr "Не здавайся!"

msgctxt "affinity_change_bark_healing_participating_dd+flagellant"
msgid "Make an offering of this flesh. Purify yourself!"
msgstr "Принеси цю плоть у жертву й очисться!"

msgctxt "affinity_change_bark_healing_participating_dd+grave_robber"
msgid "You're of great use to us. Not yet."
msgstr "Ти нам часто стаєш у нагоді. Ще рано помирати."

msgctxt "affinity_change_bark_healing_participating_dd+hellion"
msgid "A weakling you are not!"
msgstr "У тобі є сили!"

msgctxt "affinity_change_bark_healing_participating_dd+highwayman"
msgid "Get up. You're tougher than this."
msgstr "Піднімайся, тобі це під силу."

msgctxt "affinity_change_bark_healing_participating_dd+jester"
msgid "You can't leave me short-handed."
msgstr "Без тебе я мов без рук."

msgctxt "affinity_change_bark_healing_participating_dd+leper"
msgid "Our journey has not reached its end."
msgstr "Наш похід ще не завершено."

msgctxt "affinity_change_bark_healing_participating_dd+man_at_arms"
msgid "On your feet!"
msgstr "Вставай!"

msgctxt "affinity_change_bark_healing_participating_dd+occultist"
msgid "It is not your time."
msgstr "Твій час ще не настав!"

msgctxt "affinity_change_bark_healing_participating_dd+plague_doctor"
msgid "I conquered death once - I can do so again!"
msgstr "Якось я вже здолала смерть — і зроблю це ще раз!"

msgctxt "affinity_change_bark_healing_participating_dd+runaway"
msgid "You're no quitter!"
msgstr "Ти хоробра людина!"

msgctxt "affinity_change_bark_healing_participating_dd+vestal"
msgid "Trust in the Light and each other."
msgstr "Будемо вірити в друзів і силу Світла."

msgctxt "affinity_change_bark_healing_participating_wounded@0"
msgid "That one looks deep."
msgstr "Здається, рана глибока."

msgctxt "affinity_change_bark_healing_participating_wounded@1"
msgid "You're still in this fight, friend!"
msgstr "Ти ще можеш битися!"

msgctxt "affinity_change_bark_healing_participating_wounded+flagellant"
msgid "Seek absolution and ye shall find."
msgstr "Хто шукає відпущення гріхів, той знайде."

msgctxt "affinity_change_bark_healing_participating_wounded+grave_robber"
msgid "T'ain't pretty, but it will hold."
msgstr "Кепсько, але ти протримаєшся."

msgctxt "affinity_change_bark_healing_participating_wounded+hellion"
msgid "We look after one another!"
msgstr "Ми дбаємо про одне одного!"

msgctxt "affinity_change_bark_healing_participating_wounded+highwayman"
msgid "Can't leave you hangin', can I?"
msgstr "Хіба я можу залишити тебе в біді?"

msgctxt "affinity_change_bark_healing_participating_wounded+jester"
msgid "We'll fix it up proper on the coach later."
msgstr "Ми перев’яжемо рани потім, у Диліжансі."

msgctxt "affinity_change_bark_healing_participating_wounded+leper"
msgid "You take punishment well."
msgstr "Ти стійко переносиш покарання."

msgctxt "affinity_change_bark_healing_participating_wounded+man_at_arms"
msgid "What are comrades for?"
msgstr "Для чого ще потрібні соратники?"

msgctxt "affinity_change_bark_healing_participating_wounded+occultist"
msgid "This may hurt."
msgstr "Може бути боляче."

msgctxt "affinity_change_bark_healing_participating_wounded+plague_doctor"
msgid "None better for your wounds than I!"
msgstr "Ніхто краще за мене тебе не залатає."

msgctxt "affinity_change_bark_healing_participating_wounded+runaway"
msgid "Can't have you flagging!"
msgstr "Твоє полум’я не згасне!"

msgctxt "affinity_change_bark_healing_participating_wounded+vestal"
msgid "Let me read you a verse..."
msgstr "Я прочитаю тобі вірш..."

msgctxt "affinity_change_bark_healing_participating+flagellant"
msgid "Every wound a doorway to the divine."
msgstr "Кожна рана відкриває шлях до божественного."

msgctxt "affinity_change_bark_healing_participating+grave_robber"
msgid "There. Good as gold."
msgstr "Ось. На вагу золота."

msgctxt "affinity_change_bark_healing_participating+hellion"
msgid "You will spill more blood yet!"
msgstr "Ти ще проллєш кров!"

msgctxt "affinity_change_bark_healing_participating+highwayman"
msgid "What's a little blood, eh."
msgstr "Та це подряпина."

msgctxt "affinity_change_bark_healing_participating+jester"
msgid "I'll fix that if you stop bleeding on me."
msgstr "Я все зроблю, якщо ти не капатимеш на мене кров’ю."

msgctxt "affinity_change_bark_healing_participating+leper"
msgid "This pain of yours - it has a purpose."
msgstr "Твій біль не марний."

msgctxt "affinity_change_bark_healing_participating+man_at_arms"
msgid "On you go!"
msgstr "Ні кроку назад!"

msgctxt "affinity_change_bark_healing_participating+occultist"
msgid "Allow me."
msgstr "Дозволь мені."

msgctxt "affinity_change_bark_healing_participating+plague_doctor"
msgid "Cease your wriggling and let me work!"
msgstr "Припини смикати, бо буде боляче."

msgctxt "affinity_change_bark_healing_participating+runaway"
msgid "Lemme get that."
msgstr "Дай-но мені."

msgctxt "affinity_change_bark_healing_participating+vestal"
msgid "Close your eyes a moment, friend."
msgstr "Заплющ очі на хвилинку."

msgctxt "affinity_change_bark_moved_me_negative@0"
msgid "Pardon me?"
msgstr "Перепрошую?"

msgctxt "affinity_change_bark_moved_me_negative@1"
msgid "You trample in the line of fire!"
msgstr "Ти стоїш на лінії вогню!"

msgctxt "affinity_change_bark_moved_me_negative+flagellant"
msgid "Meddle not with my destiny! Away!"
msgstr "Моя доля — це мій вибір! Відійди!"

msgctxt "affinity_change_bark_moved_me_negative+grave_robber"
msgid "You oaf! They'll spy me now!"
msgstr "Через тебе мене помітять!"

msgctxt "affinity_change_bark_moved_me_negative+hellion"
msgid "You have not earned the right!"
msgstr "Ти не маєш на це права!"

msgctxt "affinity_change_bark_moved_me_negative+highwayman"
msgid "I was lining up a shot!"
msgstr "Я хотів стріляти!"

msgctxt "affinity_change_bark_moved_me_negative+jester"
msgid "What's the matter - don't trust me?"
msgstr "Що таке? Ти не довіряєш мені?"

msgctxt "affinity_change_bark_moved_me_negative+leper"
msgid "I know where I am most useful."
msgstr "Я сам знаю, де від мене більше користі."

msgctxt "affinity_change_bark_moved_me_negative+man_at_arms"
msgid "You're out of formation!"
msgstr "Тримаймося разом!"

msgctxt "affinity_change_bark_moved_me_negative+occultist"
msgid "You presume much."
msgstr "Ти занадто багато собі дозволяєш."

msgctxt "affinity_change_bark_moved_me_negative+plague_doctor"
msgid "What is the meaning of this?!"
msgstr "І що це має означати?"

msgctxt "affinity_change_bark_moved_me_negative+runaway"
msgid "I don't recall takin' orders from you."
msgstr "Не пригадаю, щоб я дозволяла собою командувати."

msgctxt "affinity_change_bark_moved_me_negative+vestal"
msgid "Cease this nervous skittering!"
msgstr "Досить тут бігати!"

msgctxt "affinity_change_bark_moved_me_positive@0"
msgid "Pardon, friend."
msgstr "Пробач, друже."

msgctxt "affinity_change_bark_moved_me_positive@1"
msgid "This way to victory!"
msgstr "Це шлях до перемоги!"

msgctxt "affinity_change_bark_moved_me_positive+flagellant"
msgid "Know humility and let me through!"
msgstr "Пізнай смиренність і пропусти мене!"

msgctxt "affinity_change_bark_moved_me_positive+grave_robber"
msgid "Let me slip past, hm?"
msgstr "Дозволь проскочити."

msgctxt "affinity_change_bark_moved_me_positive+hellion"
msgid "We can rush them from anywhere!"
msgstr "Ми дістанемо ворога де завгодно!"

msgctxt "affinity_change_bark_moved_me_positive+highwayman"
msgid "A good ambush is all preparation."
msgstr "Гарна засідка потребує підготовки."

msgctxt "affinity_change_bark_moved_me_positive+jester"
msgid "And a one and a two..."
msgstr "І раз, і два..."

msgctxt "affinity_change_bark_moved_me_positive+leper"
msgid "I am needed here."
msgstr "Я потрібен тут."

msgctxt "affinity_change_bark_moved_me_positive+man_at_arms"
msgid "Positions, all! Form up!"
msgstr "На позиції, шикуйсь!"

msgctxt "affinity_change_bark_moved_me_positive+occultist"
msgid "This position may prove an advantage."
msgstr "Ця позиція може забезпечити перевагу."

msgctxt "affinity_change_bark_moved_me_positive+plague_doctor"
msgid "I have an idea!"
msgstr "У мене ідея!"

msgctxt "affinity_change_bark_moved_me_positive+runaway"
msgid "Trust me on this!"
msgstr "Довірся мені!"

msgctxt "affinity_change_bark_moved_me_positive+vestal"
msgid "I know what I'm doing!"
msgstr "Я знаю, що роблю!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous@0"
msgid "My trusted ally, indeed..."
msgstr "Мій вірний союзник..."

msgctxt "affinity_change_bark_remove_dot_observing_jealous@1"
msgid "That ought to have been mine!"
msgstr "Це було потрібно мені!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded@0"
msgid "As I waste away...!"
msgstr "Так, я чахну..."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded@1"
msgid "Showing your true colours, there."
msgstr "Ти показуєш своє справжнє обличчя."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+flagellant"
msgid "You play at strategy like a child with toys."
msgstr "Ти складаєш стратегію, мов дитина, що бавиться іграшками."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+grave_robber"
msgid "Left to my own devices once more!"
msgstr "Знов мене залишили напризволяще!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+hellion"
msgid "Who shall hold the line, then?!"
msgstr "Хто ж триматиме стрій?"

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+highwayman"
msgid "Not the first time I've been left bleeding."
msgstr "Я вже не вперше залишаюся без допомоги."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+jester"
msgid "I'll sort myself out, then."
msgstr "Я сам з усім розберуся."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+leper"
msgid "Hmph."
msgstr "Ось як..."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+man_at_arms"
msgid "A unit is only as strong as its weakest arm..."
msgstr "Сила загону визначається його найслабшим солдатом."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+occultist"
msgid "I have a long memory..."
msgstr "У мене гарна пам’ять..."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+plague_doctor"
msgid "You FOOL!"
msgstr "От нетяма!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+runaway"
msgid "Thought you had my back."
msgstr "Я вважала, на тебе можна покластися."

msgctxt "affinity_change_bark_remove_dot_observing_jealous_wounded+vestal"
msgid "At any moment they shall break us!"
msgstr "Вони нас здолають нас у будь-який момент!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous+flagellant"
msgid "My blood alone is worth two of them! "
msgstr "Сама лише моя кров дорожча за двох із них! "

msgctxt "affinity_change_bark_remove_dot_observing_jealous+grave_robber"
msgid "Wherever are your manners?"
msgstr "Де твої манери?"

msgctxt "affinity_change_bark_remove_dot_observing_jealous+hellion"
msgid "Don't let me grow weak...!"
msgstr "Не дай мені ослабнути..."

msgctxt "affinity_change_bark_remove_dot_observing_jealous+highwayman"
msgid "Share the wealth, eh?"
msgstr "Поділишся багатством?"

msgctxt "affinity_change_bark_remove_dot_observing_jealous+jester"
msgid "Think me useless, do you?"
msgstr "Гадаєш, від мене жодної користі?!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous+leper"
msgid "I require aid same as any hale soldier."
msgstr "Мені потрібна допомога, як і будь-якому солдатові."

msgctxt "affinity_change_bark_remove_dot_observing_jealous+man_at_arms"
msgid "I 'ad use for that!"
msgstr "Я б від такого теж не відмовився!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous+occultist"
msgid "Test fate at your own risk."
msgstr "Випробуй долю на свій страх і ризик."

msgctxt "affinity_change_bark_remove_dot_observing_jealous+plague_doctor"
msgid "Cease these nonsense speculations!"
msgstr "Припини ці дурниці!"

msgctxt "affinity_change_bark_remove_dot_observing_jealous+runaway"
msgid "I ought to leave you in the dust..."
msgstr "Я тобі ще втру носа..."

msgctxt "affinity_change_bark_remove_dot_observing_jealous+vestal"
msgid "By risking me you imperil us all!"
msgstr "Ти наражаєш на небезпеку всіх нас!"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous@0"
msgid "Here I suffer, unaided..."
msgstr "Ніхто не полегшить мої страждання..."

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous@1"
msgid "While I breathe my last...?"
msgstr "Мені залишився лише останній подих."

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+flagellant"
msgid "My glory is foretold, yet you would condemn us!"
msgstr "Мені славу напророчили, а з твого боку можна чекати лише осуду!"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+grave_robber"
msgid "P... please, no! You'd leave me to die?!"
msgstr "Н-ні! Ти ж не залишиш мене помирати?!"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+hellion"
msgid "You'd sacrifice me?!"
msgstr "Ти пожертвуєш мною?!"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+highwayman"
msgid "Fine. Leave me to the fate I deserve..."
msgstr "Он як... Залиш мене на поталу долі, яку я заслужив..."

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+jester"
msgid "So it's come to this."
msgstr "От і дійшло до цього."

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+leper"
msgid "Passing judgment on who lives and dies?"
msgstr "Будеш вирішувати, кому жити, а кому помирати?"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+man_at_arms"
msgid "I see my trust was misplaced."
msgstr "Здається, я довірився не тій людині."

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+occultist"
msgid "Unwise. "
msgstr "Яке безглуздя. "

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+plague_doctor"
msgid "You'll be sorry when I'm gone!"
msgstr "Ти ще пошкодуєш, коли мене не стане!"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+runaway"
msgid "I trusted you! NO!!"
msgstr "Я тобі довіряла! НІ!!!"

msgctxt "affinity_change_bark_remove_dot_observing_mortally_jealous+vestal"
msgid "Left me to fend for myself, have you?"
msgstr "Залишаєш мене на поталу долі?"

msgctxt "affinity_change_bark_remove_dot_participating@0"
msgid "For your agonies."
msgstr "За твої страждання."

msgctxt "affinity_change_bark_remove_dot_participating@1"
msgid "That looks sore."
msgstr "Тобі, певно, боляче."

msgctxt "affinity_change_bark_remove_dot_participating@2"
msgid "Wither not!"
msgstr "Не кисни!"

msgctxt "affinity_change_bark_remove_dot_participating_dd@0"
msgid "Clamber back from the precipice!"
msgstr "Вибирайся!"

msgctxt "affinity_change_bark_remove_dot_participating_dd@1"
msgid "Return to us!"
msgstr "Повертайся до нас!"

msgctxt "affinity_change_bark_remove_dot_participating_dd+flagellant"
msgid "You dance on the threshold - here lies power!"
msgstr "Ти танцюєш на порозі смерті — це джерело сили!"

msgctxt "affinity_change_bark_remove_dot_participating_dd+grave_robber"
msgid "I'm no physic, but let me try!"
msgstr "Я не лікар, але спробую."

msgctxt "affinity_change_bark_remove_dot_participating_dd+hellion"
msgid "Is it not freeing, standing on the edge?!"
msgstr "Стояти на краю — це незабутнє відчуття."

msgctxt "affinity_change_bark_remove_dot_participating_dd+highwayman"
msgid "Bite down on somethin'. This might hurt."
msgstr "Візьми щось у зуби, буде боляче."

msgctxt "affinity_change_bark_remove_dot_participating_dd+jester"
msgid "Cuttin' it close, friend."
msgstr "Ледве встиг, друже."

msgctxt "affinity_change_bark_remove_dot_participating_dd+leper"
msgid "Playing dice with fortune herself."
msgstr "Граю в кості із самою долею."

msgctxt "affinity_change_bark_remove_dot_participating_dd+man_at_arms"
msgid "You ain't dismissed yet!"
msgstr "На тобі ще не поставили хреста!"

msgctxt "affinity_change_bark_remove_dot_participating_dd+occultist"
msgid "Thought we'd lost you."
msgstr "Думав, ми втратили тебе."

msgctxt "affinity_change_bark_remove_dot_participating_dd+plague_doctor"
msgid "Move not an inch! I'm working!"
msgstr "Не підходити, я працюю!"

msgctxt "affinity_change_bark_remove_dot_participating_dd+runaway"
msgid "Don't scare us like that."
msgstr "Більше не лякай нас так."

msgctxt "affinity_change_bark_remove_dot_participating_dd+vestal"
msgid "Even dire wounds can be mended."
msgstr "Навіть тяжкі рани можна вилікувати."

msgctxt "affinity_change_bark_remove_dot_participating_wounded@0"
msgid "No! I will not allow this!"
msgstr "Ні, не дозволю!"

msgctxt "affinity_change_bark_remove_dot_participating_wounded@1"
msgid "The pain persists, but you'll live!"
msgstr "Хай болить, але ти житимеш!"

msgctxt "affinity_change_bark_remove_dot_participating_wounded+flagellant"
msgid "Shrink ye not from what greatness asks!"
msgstr "Не цурайтеся того, чого вимагає велич!"

msgctxt "affinity_change_bark_remove_dot_participating_wounded+grave_robber"
msgid "Here, lest ye grow faint."
msgstr "Тримай, бо ослабнеш."

msgctxt "affinity_change_bark_remove_dot_participating_wounded+hellion"
msgid "Foul spirits! Miasma in the air!"
msgstr "У повітрі міазми!"

msgctxt "affinity_change_bark_remove_dot_participating_wounded+highwayman"
msgid "That looks nasty. Here."
msgstr "Виглядає погано. Тримай."

msgctxt "affinity_change_bark_remove_dot_participating_wounded+jester"
msgid "Fine. From my stash."
msgstr "Гаразд. З моєї заначки."

msgctxt "affinity_change_bark_remove_dot_participating_wounded+leper"
msgid "Wise not to let this fester."
msgstr "Краще не доводити до нагноєння."

msgctxt "affinity_change_bark_remove_dot_participating_wounded+man_at_arms"
msgid "Mind your wounds!"
msgstr "Займися своїми ранами!"

msgctxt "affinity_change_bark_remove_dot_participating_wounded+occultist"
msgid "Don't let that run rampant."
msgstr "Не треба із цим зволікати."

msgctxt "affinity_change_bark_remove_dot_participating_wounded+plague_doctor"
msgid "We caught this in time. Here!"
msgstr "Ми встигли. Тримай!"

msgctxt "affinity_change_bark_remove_dot_participating_wounded+runaway"
msgid "That could spread. Let me."
msgstr "Може погіршати. Давай допоможу."

msgctxt "affinity_change_bark_remove_dot_participating_wounded+vestal"
msgid "You require aid - I provide it."
msgstr "Тобі потрібна допомога — я допомагаю."

msgctxt "affinity_change_bark_remove_dot_participating+flagellant"
msgid "Draw strength from this suffering!"
msgstr "Черпайте силу із цього страждання!"

msgctxt "affinity_change_bark_remove_dot_participating+grave_robber"
msgid "You're in fine hands - promise."
msgstr "Ти в добрих руках, запевняю."

msgctxt "affinity_change_bark_remove_dot_participating+hellion"
msgid "This mud cures maladies! Trust me!"
msgstr "Це лікувальні грязі, повір!"

msgctxt "affinity_change_bark_remove_dot_participating+highwayman"
msgid "Pay me back later, eh?"
msgstr "Борг повернеш пізніше, добре?"

msgctxt "affinity_change_bark_remove_dot_participating+jester"
msgid "Not your finest moment. Here."
msgstr "Ти не в найкращій формі. Тримай."

msgctxt "affinity_change_bark_remove_dot_participating+leper"
msgid "We prevent suffering where we can."
msgstr "Страждань краще уникнути, якщо це можливо."

msgctxt "affinity_change_bark_remove_dot_participating+man_at_arms"
msgid "That ought to do it!"
msgstr "Це має спрацювати!"

msgctxt "affinity_change_bark_remove_dot_participating+occultist"
msgid "Be still! This is my specialty!"
msgstr "Сиди тихо: це справа для мене!"

msgctxt "affinity_change_bark_remove_dot_participating+plague_doctor"
msgid "I'm sure I have an ampoule for that..."
msgstr "У мене є ампула для таких випадків..."

msgctxt "affinity_change_bark_remove_dot_participating+runaway"
msgid "I can patch that up!"
msgstr "Я все виправлю!"

msgctxt "affinity_change_bark_remove_dot_participating+vestal"
msgid "Let the warmth suffuse you..."
msgstr "Нехай твоє тіло сповниться теплом..."

msgctxt "affinity_change_bark_revenge_from_crit@0"
msgid "Before it can call any allies!"
msgstr "Швидше, доки не з’явилася підмога!"

msgctxt "affinity_change_bark_revenge_from_crit@1"
msgid "They'll be sorry!"
msgstr "Вони пошкодують!"

msgctxt "affinity_change_bark_revenge_from_crit+flagellant"
msgid "The curs hunger for my sacred blood!"
msgstr "Прокляттям потрібна моя священна кров!"

msgctxt "affinity_change_bark_revenge_from_crit+grave_robber"
msgid "Marvelous. Make them bleed!"
msgstr "Чудово. Нехай ллється кров!"

msgctxt "affinity_change_bark_revenge_from_crit+hellion"
msgid "Smell that! The wind turns! "
msgstr "Відчуваєте? Вітер змінився! "

msgctxt "affinity_change_bark_revenge_from_crit+highwayman"
msgid "Gut the bastards."
msgstr "Покінчи із цими виродками!"

msgctxt "affinity_change_bark_revenge_from_crit+jester"
msgid "I'm touched. "
msgstr "Я зворушений. "

msgctxt "affinity_change_bark_revenge_from_crit+leper"
msgid "Even a worm will turn - and you are no worm."
msgstr "Навіть гнаний пацюк може огризатися, а ти не пацюк."

msgctxt "affinity_change_bark_revenge_from_crit+man_at_arms"
msgid "Show them your mettle!"
msgstr "Хай вони відчують твою силу духу."

msgctxt "affinity_change_bark_revenge_from_crit+occultist"
msgid "Deepest thanks."
msgstr "Щиро дякую."

msgctxt "affinity_change_bark_revenge_from_crit+plague_doctor"
msgid "You'd do that for me...?"
msgstr "Ти робиш це для мене?"

msgctxt "affinity_change_bark_revenge_from_crit+runaway"
msgid "I wouldn't ask that of you... but thanks."
msgstr "Не варто було... але дякую."

msgctxt "affinity_change_bark_revenge_from_crit+vestal"
msgid "Only together do we stand a chance!"
msgstr "Лише коли ми разом, у нас є шанс!"

msgctxt "affinity_change_bark_revenge_from_hit@0"
msgid "Much appreciated, friend."
msgstr "Красно дякую."

msgctxt "affinity_change_bark_revenge_from_hit@1"
msgid "You aren't so bad."
msgstr "А ти нічого так."

msgctxt "affinity_change_bark_revenge_from_hit+flagellant"
msgid "Ensure that I bled for purpose!"
msgstr "Можеш переконатися: я сплив кров’ю недарма!"

msgctxt "affinity_change_bark_revenge_from_hit+grave_robber"
msgid "I do love it when people avenge me."
msgstr "Я люблю, коли заради мене ллють кров."

msgctxt "affinity_change_bark_revenge_from_hit+hellion"
msgid "We will best them as one!"
msgstr "Разом ми всіх здолаємо!"

msgctxt "affinity_change_bark_revenge_from_hit+highwayman"
msgid "Well, well. "
msgstr "Так-так. "

msgctxt "affinity_change_bark_revenge_from_hit+jester"
msgid "That'll do!"
msgstr "Хай буде так."

msgctxt "affinity_change_bark_revenge_from_hit+leper"
msgid "It is a blessing to fight alone no longer."
msgstr "Це щастя — мати товариша по зброї."

msgctxt "affinity_change_bark_revenge_from_hit+man_at_arms"
msgid "Seize the opening! There!"
msgstr "У ворожих лавах є прогалина!"

msgctxt "affinity_change_bark_revenge_from_hit+occultist"
msgid "Were you using me as bait? Hah."
msgstr "Мене використовували як наживку? Он як..."

msgctxt "affinity_change_bark_revenge_from_hit+plague_doctor"
msgid "Skill and coordination!"
msgstr "Ось що таке майстерність і координація!"

msgctxt "affinity_change_bark_revenge_from_hit+runaway"
msgid "Phew, that'll show 'em."
msgstr "Ворогу буде непереливки."

msgctxt "affinity_change_bark_revenge_from_hit+vestal"
msgid "Light, smile upon this one."
msgstr "Світло, посміхнися цьому."

msgctxt "affinity_change_bark_stress_heal_participating@0"
msgid "There will be time for grief later."
msgstr "Зараз не час для смутку."

msgctxt "affinity_change_bark_stress_heal_participating@1"
msgid "You cannot afford to dwell on this!"
msgstr "Не можна жити минулим!"

msgctxt "affinity_change_bark_stress_heal_participating_stressed@0"
msgid "All is not lost."
msgstr "Ще не все втрачено."

msgctxt "affinity_change_bark_stress_heal_participating_stressed@1"
msgid "Breathe in. Focus."
msgstr "Подихай і зосередься."

msgctxt "affinity_change_bark_stress_heal_participating_stressed@2"
msgid "Steel your mind. The battle is not yet over."
msgstr "Кріпись, бій ще не закінчений."

msgctxt "affinity_change_bark_stress_heal_participating_stressed@3"
msgid "We need you!"
msgstr "Нам потрібна твоя допомога!"

msgctxt "affinity_change_bark_stress_heal_participating_stressed+flagellant"
msgid "Sip of my blood and know peace."
msgstr "Скуштуй моєї крові й пізнай спокій."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+grave_robber"
msgid "A faintheart you are not."
msgstr "А ти не з полохливих."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+hellion"
msgid "Oust the fear from your heart!"
msgstr "Вижени страх із серця!"

msgctxt "affinity_change_bark_stress_heal_participating_stressed+highwayman"
msgid "One foot in front of the other."
msgstr "Ось так, крок за кроком."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+jester"
msgid "Don't count yourself out."
msgstr "Не списуй себе з рахунків."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+leper"
msgid "Trust in me if you can't trust yourself."
msgstr "Якщо не довіряєш собі, вір мені."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+man_at_arms"
msgid "We leave none behind!"
msgstr "Ми нікого не залишимо позаду!"

msgctxt "affinity_change_bark_stress_heal_participating_stressed+occultist"
msgid "Move beyond fear. I know you can."
msgstr "Перебори страх. Я знаю, ти можеш."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+plague_doctor"
msgid "We will cure this world."
msgstr "Ми вилікуємо цей світ."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+runaway"
msgid "Hold on - just a little longer."
msgstr "Протримайся ще трохи..."

msgctxt "affinity_change_bark_stress_heal_participating_stressed+vestal"
msgid "I see the good in you."
msgstr "Я бачу в тобі добро."

msgctxt "affinity_change_bark_stress_heal_participating+flagellant"
msgid "Wonders beyond compare await us."
msgstr "На нас чекають неймовірні дива."

msgctxt "affinity_change_bark_stress_heal_participating+grave_robber"
msgid "Keep your eyes on the prize."
msgstr "Пам’ятай про винагороду!"

msgctxt "affinity_change_bark_stress_heal_participating+hellion"
msgid "I'll drag you through this myself!"
msgstr "Я допоможу тобі минути всі біди!"

msgctxt "affinity_change_bark_stress_heal_participating+highwayman"
msgid "You're a sturdy sort - press on."
msgstr "Ти можеш усе витримати. Не здавайся."

msgctxt "affinity_change_bark_stress_heal_participating+jester"
msgid "Think on it - you ain't died yet."
msgstr "Подумай про те, що ти ще живеш."

msgctxt "affinity_change_bark_stress_heal_participating+leper"
msgid "Much beauty remains in this world."
msgstr "На цьому світі ще є стільки всього прекрасного."

msgctxt "affinity_change_bark_stress_heal_participating+man_at_arms"
msgid "None doubt you."
msgstr "Я вірю в тебе."

msgctxt "affinity_change_bark_stress_heal_participating+occultist"
msgid "Your dismay echoes my own - but onward!"
msgstr "Я боюся так само, як і ти... але вперед!"

msgctxt "affinity_change_bark_stress_heal_participating+plague_doctor"
msgid "Verdure and vigour! "
msgstr "Свіжість і міць! "

msgctxt "affinity_change_bark_stress_heal_participating+runaway"
msgid "Like water off a duck!"
msgstr "Як з гуся вода!"

msgctxt "affinity_change_bark_stress_heal_participating+vestal"
msgid "There is comfort in times of unease."
msgstr "Заспокоєння настає навіть у скрутні часи."

msgctxt "affinity_change_crit"
msgid "CRIT"
msgstr "КРТ"

msgctxt "affinity_change_total"
msgid "${current} / ${max}"
msgstr "${current} / ${max}"

msgctxt "affinity_leaning_level_friendly"
msgid "<color=#{positive_quirk}>Friendly</color>"
msgstr "<color=#{positive_quirk}>Дружелюбність</color>"

msgctxt "affinity_leaning_level_maximum"
msgid "<color=#{positive_quirk}>Max Affinity</color>"
msgstr "<color=#{positive_quirk}>Макс. дружелюбн.</color>"

msgctxt "affinity_leaning_level_minimum"
msgid "<color=#{negative_quirk_dark}>Zero Affinity</color>"
msgstr "<color=#{negative_quirk_dark}>Макс. неприязнь</color>"

msgctxt "affinity_leaning_level_neutral"
msgid "Neutral"
msgstr "Байдужість"

msgctxt "affinity_leaning_level_unfriendly"
msgid "<color=#{negative_quirk_dark}>Unfriendly</color>"
msgstr "<color=#{negative_quirk_dark}>Неприязнь</color>"

msgctxt "affinity_leaning_level_very_friendly"
msgid "<color=#{positive_quirk}>Very Friendly</color>"
msgstr "<color=#{positive_quirk}>Симпатія</color>"

msgctxt "affinity_leaning_level_very_unfriendly"
msgid "<color=#{negative_quirk_dark}>Very Unfriendly</color>"
msgstr "<color=#{negative_quirk_dark}>Ворожість</color>"

msgctxt "affinity_tick_relationship_enter_label"
