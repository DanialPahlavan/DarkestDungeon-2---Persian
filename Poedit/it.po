msgid "<sprite name={q}icon_upgraded_skill{q}>Press @{UpgradedSkillInfo} to Show Upgraded Skill "
msgstr "<sprite name={q}icon_upgraded_skill{q}>Premi @{UpgradedSkillInfo} per mostrare l’abilità potenziata "

msgctxt "gdpr_dialog_continue_label"
msgid "Continue"
msgstr "Continua"

