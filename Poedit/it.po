msgid "The <color=#{notable}>Inn</color> is where heroes can rest and recover between regions."
msgstr "Nella <color=#{notable}>Locanda</color> gli eroi possono riprendersi e riposare tra una regione e l’altra."

msgctxt "tutorial_t1600_title"
msgid "The Inn"
msgstr "La Locanda"

#, fuzzy
#| msgctxt "tutorial_t1601_description"
#| msgid ""
#| "<color=#{harmful}>INN ITEMS</color> can only be used at the <color=#{harmful}>INN</color>. They can heal, de-"
