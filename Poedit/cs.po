msgctxt "story_bark_gauntchirurgeon_shadow_born_1"
msgid "A colourless place..."
msgstr "Bezútěšné místo..."

msgctxt "story_bark_gauntchirurgeon_shroud_tactician_1"
msgid "Weaker foes rely upon surprise..."
msgstr "Slabší protivníci spoléhají na překvapení..."

#, fuzzy
#| msgctxt "bark_node_exit_storycosmic+runaway@0"
#| msgid "Let's get far away from this place."
msgctxt "story_bark_gauntchirurgeon_slippery_1"
msgid "Fortune guide us from this place..."
msgstr "Štěstěna nás z tohoto místa vyvede..."

msgctxt "story_bark_gauntchirurgeon_sneaky_1"
msgid "For once, the shadows are on <i>our</i> side..."
msgstr "Pro jednou jsou stíny na <i>naší</i> straně..."

#, fuzzy
