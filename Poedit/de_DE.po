#| msgid "Early results are... encouraging!"
msgctxt "vo_battle_hero_action_kill_monster_first_03_0"
msgid "Early results... are encouraging!"
msgstr "Die ersten Ergebnisse sind … durchaus ermutigend!"

msgctxt "vo_battle_hero_action_kill_monster_first_04_0"
msgid "On to the next!"
msgstr "Weiter geht es!"

msgctxt "vo_battle_hero_action_kill_monster_general_01_0"
msgid "One less obstacle in our path."
msgstr "Ein Hindernis haben wir aus dem Weg geräumt."

msgctxt "vo_battle_hero_action_kill_monster_general_02_0"
msgid "Another impediment cleared with impunity."
msgstr "Und wieder eine Hürde mit Bravour genommen."

msgctxt "vo_battle_hero_action_kill_monster_general_03_0"
msgid "Messy, but effective."
msgstr "Unschön anzusehen, aber erfolgreich."

msgctxt "vo_battle_hero_action_kill_monster_general_04_0"
msgid "The work continues..."
msgstr "Wir haben noch mehr Arbeit vor uns …"

msgctxt "vo_battle_hero_action_kill_monster_general_05_0"
msgid "A righteous action!"
msgstr "Eine rechtschaffene Tat!"

#, fuzzy
#| msgctxt "vo_battle_hero_action_kill_monster_general_06_0"
#| msgid "A petty hinderance!"
msgctxt "vo_battle_hero_action_kill_monster_general_06_0"
msgid "A petty hinderance!"
msgstr "Ein triviales Hindernis!"

msgctxt "vo_battle_hero_action_kill_monster_general_07_0"
msgid "A simple variable, easily resolved."
msgstr "Eine einfach aufzulösende Variable."

msgctxt "vo_battle_hero_action_kill_monster_general_08_0"
msgid "A lesson taught is a lesson learned."
msgstr "Eine Lektion wurde erteilt und gelernt."

msgctxt "vo_battle_hero_action_kill_monster_general_09_0"
msgid "Straightforward, and effective!"
msgstr "Unkompliziert und wirksam!"

msgctxt "vo_battle_hero_action_kill_monster_strong_01_0"
msgid "A resounding response!"
msgstr "Ein großer Erfolg!"

msgctxt "vo_battle_hero_action_kill_monster_strong_02_0"
msgid "They falter - so press your advance!"
msgstr "Sie sind geschwächt, nutze deinen Vorteil!"

msgctxt "vo_battle_hero_action_kill_monster_strong_03_0"
msgid "Take heart! And do not relent!"
msgstr "Schöpf neuen Mut und sei unerbittlich!"

msgctxt "vo_battle_hero_action_kill_monster_weak_01_0"
msgid "A trivial worriment..."
msgstr "Kaum der Sorge wert …"

msgctxt "vo_battle_hero_action_kill_monster_weak_02_0"
msgid "Easily dispatched!"
msgstr "Ein leichtes Opfer!"

#, fuzzy
#| msgctxt "vo_battle_hero_action_kill_monster_weak_03_0"
#| msgid "A trifiling matter..."
msgctxt "vo_battle_hero_action_kill_monster_weak_03_0"
msgid "A trifling matter..."
msgstr "Ein unbedeutendes Problem …"

#, fuzzy
#| msgctxt "minimap_tooltip_cemetery"
